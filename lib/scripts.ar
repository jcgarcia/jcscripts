#!/bin/bash

# Clean empty or nearly-empty backup directories
# Usage: clean-empty-backups [--dry-run] [--local|--nas|--all]

# Configuration
LOCAL_DEST="/media/jcgarcia/F08290EB8290B794/backup/docs-backup"
NAS_DEST="/mnt/nas/docs-backup"
MIN_SIZE_KB=100  # Minimum size in KB to consider valid (100KB)

# Colors
RED='\033[0;31m'
GREEN='\033[0;32m'
YELLOW='\033[1;33m'
BLUE='\033[0;34m'
NC='\033[0m'

# Flags
DRY_RUN=false
CLEAN_LOCAL=true
CLEAN_NAS=true

# Parse arguments
while [[ $# -gt 0 ]]; do
    case "$1" in
        --dry-run)
            DRY_RUN=true
            shift
            ;;
        --local)
            CLEAN_LOCAL=true
            CLEAN_NAS=false
            shift
            ;;
        --nas)
            CLEAN_LOCAL=false
            CLEAN_NAS=true
            shift
            ;;
        --all)
            CLEAN_LOCAL=true
            CLEAN_NAS=true
            shift
            ;;
        -h|--help)
            echo "Usage: clean-empty-backups [OPTIONS]"
            echo ""
            echo "Clean empty or nearly-empty backup directories"
            echo ""
            echo "Options:"
            echo "  --dry-run   Show what would be deleted without actually deleting"
            echo "  --local     Clean only local disk backups"
            echo "  --nas       Clean only NAS backups"
            echo "  --all       Clean all locations (default)"
            echo "  -h, --help  Show this help message"
            echo ""
            echo "Configuration:"
            echo "  Minimum valid size: ${MIN_SIZE_KB}KB"
            exit 0
            ;;
        *)
            echo "Unknown option: $1"
            echo "Use -h or --help for usage information"
            exit 1
            ;;
    esac
done

if [ "$DRY_RUN" = true ]; then
    echo -e "${YELLOW}=== DRY RUN MODE - No files will be deleted ===${NC}"
    echo ""
fi

# Function to clean empty backups in a directory
clean_empty_backups() {
    local location=$1
    local path=$2
    local deleted_count=0
    local total_count=0
    
    echo -e "${BLUE}Checking $location${NC}"
    echo "Path: $path"
    
    if [ ! -d "$path" ]; then
        echo -e "${YELLOW}  ⚠ Directory not accessible${NC}"
        echo ""
        return
    fi
    
    echo ""
    
    # Find all backup directories
    while IFS= read -r backup; do
        if [ -n "$backup" ]; then
            ((total_count++))
            local name=$(basename "$backup")
            local size_kb=$(sudo du -sk "$backup" 2>/dev/null | cut -f1)
            
            if [ -z "$size_kb" ] || [ "$size_kb" -lt "$MIN_SIZE_KB" ]; then
                local size_human=$(sudo du -sh "$backup" 2>/dev/null | cut -f1)
                
                if [ "$DRY_RUN" = true ]; then
                    echo -e "${YELLOW}  [DRY RUN] Would delete: $name ($size_human)${NC}"
                else
                    echo -e "${RED}  Deleting: $name ($size_human)${NC}"
                    sudo rm -rf "$backup"
                    
                    if [ $? -eq 0 ]; then
                        ((deleted_count++))
                    else
                        echo -e "${RED}    ✗ Failed to delete${NC}"
                    fi
                fi
            fi
        fi
    done < <(sudo ls -d "$path"/*/ 2>/dev/null)
    
    echo ""
    if [ "$DRY_RUN" = true ]; then
        echo -e "${CYAN}Total: $total_count backups checked${NC}"
    else
        echo -e "${GREEN}✓ Deleted $deleted_count out of $total_count backups${NC}"
    fi
    echo ""
}

echo -e "${GREEN}=== Cleaning Empty Backups ===${NC}"
echo "Minimum valid size: ${MIN_SIZE_KB}KB"
echo ""

# Clean based on flags
if [ "$CLEAN_LOCAL" = true ]; then
    clean_empty_backups "Local Disk" "$LOCAL_DEST"
fi

if [ "$CLEAN_NAS" = true ]; then
    clean_empty_backups "NAS" "$NAS_DEST"
fi

echo -e "${GREEN}=== Cleanup Complete ===${NC}"

if [ "$DRY_RUN" = true ]; then
    echo ""
    echo "This was a dry run. To actually delete files, run without --dry-run"
fi

echo ""
echo "Run 'list-backups' to see remaining backups"
