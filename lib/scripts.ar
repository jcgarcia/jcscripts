#!/bin/bash

# ------------------------------------------------------------------------------
# Script Name:    setupX11Forwarding
# Description:    Automates the complete setup of X11 forwarding over SSH
#                 on Ubuntu (20.04, 22.04, 24.04) and Alpine Linux servers
# Usage:          setupX11Forwarding
# Options:
#                 -h | --help       Show this help message
#                 -v | --version    Show script version
# Author:         Julio Cesar Garcia
# Date:           2025-11-22
# Version:        2.0
# ------------------------------------------------------------------------------

VERSION="2.0"

set -e  # Exit on error
set -u  # Exit on undefined variable

# Set TERM if not set (for remote execution via SSH)
export TERM=${TERM:-dumb}

# Colors for output
RED='\033[0;31m'
GREEN='\033[0;32m'
YELLOW='\033[1;33m'
BLUE='\033[0;34m'
NC='\033[0m' # No Color

# Configuration
SSHD_CONFIG="/etc/ssh/sshd_config"
SSHD_CONFIG_BACKUP="${SSHD_CONFIG}.backup.$(date +%Y%m%d_%H%M%S)"
INSTALL_TEST_APPS=true
RESTART_SSH=true

# ------------------------------------------------------------------------------
# Helper Functions
# ------------------------------------------------------------------------------

# Display help message
show_help() {
  cat << EOF
Usage: setupX11Forwarding [OPTIONS]

Automates the complete setup of X11 forwarding over SSH on Ubuntu (20.04, 22.04, 24.04)
and Alpine Linux headless servers.

OPTIONS:
  -h, --help       Display this help message and exit
  -v, --version    Display script version and exit

DESCRIPTION:
  This script configures X11 forwarding on Ubuntu and Alpine servers by:
  - Installing required X11 packages (xauth, xdg-utils, xorg, x11-apps)
  - Configuring SSH daemon for X11 forwarding
  - Setting up user environment (.Xauthority)
  - Verifying firewall configuration
  - Testing the setup

REQUIREMENTS:
  - Must be run as root or with sudo
  - Ubuntu 20.04, 22.04, 24.04 or Alpine Linux 3.x
  - Active internet connection

EXAMPLES:
  sudo setupX11Forwarding              # Interactive setup
  sudo setupX11Forwarding -h           # Show this help
  sudo setupX11Forwarding -v           # Show version

AFTER SETUP:
  Connect from client with: ssh -X username@server
  Test with: xeyes, xclock, xcalc

AUTHOR:
  Julio Cesar Garcia

EOF
}

# Function to extract the version from the script header of the current script
extract_version() {
    local script_file="$0"  # Referring to the current script
    grep -i '^# Version:' "$script_file" | awk -F': ' '{print $2}'
}

# Helper function to display the version
show_version() {
    version=$(extract_version)
    echo "setupX11Forwarding version: ${version}"
}

print_header() {
    echo -e "\n${BLUE}========================================${NC}"
    echo -e "${BLUE}$1${NC}"
    echo -e "${BLUE}========================================${NC}\n"
}

print_success() {
    echo -e "${GREEN}✓${NC} $1"
}

print_error() {
    echo -e "${RED}✗${NC} $1"
}

print_warning() {
    echo -e "${YELLOW}⚠${NC} $1"
}

print_info() {
    echo -e "${BLUE}ℹ${NC} $1"
}

check_root() {
    if [ "$EUID" -ne 0 ]; then
        print_error "This script must be run as root or with sudo"
        exit 1
    fi
}

check_os() {
    # Detect OS type
    if [ -f /etc/os-release ]; then
        source /etc/os-release
        OS_ID="$ID"
        OS_NAME="$NAME"
        OS_VERSION="$VERSION_ID"
    elif [ -f /etc/lsb-release ]; then
        source /etc/lsb-release
        OS_ID="ubuntu"
        OS_NAME="$DISTRIB_DESCRIPTION"
        OS_VERSION="$DISTRIB_RELEASE"
    else
        print_error "Unable to detect OS type"
        exit 1
    fi
    
    print_info "Detected: $OS_NAME"
    
    case "$OS_ID" in
        ubuntu)
            PKG_MANAGER="apt"
            SERVICE_MANAGER="systemctl"
            case "$OS_VERSION" in
                20.04|22.04|24.04)
                    print_success "Ubuntu version supported"
                    ;;
                *)
                    print_warning "This Ubuntu version ($OS_VERSION) may not be fully tested"
                    ;;
            esac
            ;;
        alpine)
            PKG_MANAGER="apk"
            SERVICE_MANAGER="rc-service"
            print_success "Alpine Linux supported"
            ;;
        *)
            print_error "Unsupported OS: $OS_ID"
            print_info "This script supports Ubuntu and Alpine Linux"
            exit 1
            ;;
    esac
}

################################################################################
# Main Functions
################################################################################

update_system() {
    print_header "Updating System Package Lists"
    
    if [ "$PKG_MANAGER" = "apt" ]; then
        apt-get update -qq
    elif [ "$PKG_MANAGER" = "apk" ]; then
        apk update --quiet
    fi
    print_success "Package lists updated"
}

install_x11_packages() {
    print_header "Installing X11 Packages"
    
    if [ "$PKG_MANAGER" = "apt" ]; then
        # Ubuntu/Debian package installation
        if ! dpkg -l xauth 2>/dev/null | grep -q "^ii"; then
            print_info "Installing xauth..."
            apt-get install -y -qq xauth
            print_success "xauth installed"
        else
            print_success "xauth already installed"
        fi
        
        if ! dpkg -l xdg-utils 2>/dev/null | grep -q "^ii"; then
            print_info "Installing xdg-utils..."
            apt-get install -y -qq xdg-utils
            print_success "xdg-utils installed"
        else
            print_success "xdg-utils already installed"
        fi
        
        if ! dpkg -l x11-utils 2>/dev/null | grep -q "^ii"; then
            print_info "Installing x11-utils..."
            apt-get install -y -qq x11-utils
            print_success "x11-utils installed"
        else
            print_success "x11-utils already installed"
        fi
        
        if ! dpkg -l xorg 2>/dev/null | grep -q "^ii"; then
            print_info "Installing xorg..."
            apt-get install -y -qq xorg
            print_success "xorg installed"
        else
            print_success "xorg already installed"
        fi
        
        if [ "$INSTALL_TEST_APPS" = true ]; then
            if ! dpkg -l x11-apps 2>/dev/null | grep -q "^ii"; then
                print_info "Installing X11 test applications..."
                apt-get install -y -qq x11-apps
                print_success "X11 test apps installed (xeyes, xclock, xcalc, xterm)"
            else
                print_success "X11 test apps already installed"
            fi
        fi
    elif [ "$PKG_MANAGER" = "apk" ]; then
        # Alpine package installation
        if ! apk info -e xauth &>/dev/null; then
            print_info "Installing xauth..."
            apk add --quiet xauth
            print_success "xauth installed"
        else
            print_success "xauth already installed"
        fi
        
        if ! apk info -e xdg-utils &>/dev/null; then
            print_info "Installing xdg-utils..."
            apk add --quiet xdg-utils 2>/dev/null || print_warning "xdg-utils not available (optional)"
        else
            print_success "xdg-utils already installed"
        fi
        
        if ! apk info -e xorg-server &>/dev/null; then
            print_info "Installing xorg-server..."
            apk add --quiet xorg-server
            print_success "xorg-server installed"
        else
            print_success "xorg-server already installed"
        fi
        
        if [ "$INSTALL_TEST_APPS" = true ]; then
            local test_apps_needed=false
            if ! apk info -e xeyes &>/dev/null || ! apk info -e xclock &>/dev/null; then
                test_apps_needed=true
                print_info "Installing X11 test applications..."
                apk add --quiet xeyes xclock 2>/dev/null || print_warning "Some test apps not available"
            fi
            if [ "$test_apps_needed" = false ]; then
                print_success "X11 test apps already installed"
            fi
        fi
    fi
    
    # Verify installation
    if command -v xauth &> /dev/null; then
        print_success "xauth verified at: $(which xauth)"
    else
        print_error "xauth installation failed"
        exit 1
    fi
    
    if command -v xdg-open &> /dev/null; then
        print_success "xdg-open verified at: $(which xdg-open)"
    else
        print_warning "xdg-open installation failed (optional)"
    fi
    
    if command -v xdpyinfo &> /dev/null; then
        print_success "xdpyinfo verified at: $(which xdpyinfo)"
    else
        print_warning "xdpyinfo installation failed (optional)"
    fi
}

backup_ssh_config() {
    print_header "Backing Up SSH Configuration"
    
    if [ -f "$SSHD_CONFIG" ]; then
        cp "$SSHD_CONFIG" "$SSHD_CONFIG_BACKUP"
        print_success "SSH config backed up to: $SSHD_CONFIG_BACKUP"
    else
        print_error "SSH config file not found at: $SSHD_CONFIG"
        exit 1
    fi
}

configure_ssh_x11() {
    print_header "Configuring SSH for X11 Forwarding"
    
    # Function to set or update SSH config parameter
    update_ssh_param() {
        local param=$1
        local value=$2
        local config=$3
        
        if grep -q "^${param}" "$config"; then
            # Parameter exists, update it
            sed -i "s/^${param}.*/${param} ${value}/" "$config"
            print_info "Updated: ${param} ${value}"
        elif grep -q "^#${param}" "$config"; then
            # Parameter exists but commented, uncomment and set
            sed -i "s/^#${param}.*/${param} ${value}/" "$config"
            print_info "Enabled: ${param} ${value}"
        else
            # Parameter doesn't exist, add it
            echo "${param} ${value}" >> "$config"
            print_info "Added: ${param} ${value}"
        fi
    }
    
    # Configure X11 forwarding parameters
    update_ssh_param "X11Forwarding" "yes" "$SSHD_CONFIG"
    update_ssh_param "X11DisplayOffset" "10" "$SSHD_CONFIG"
    update_ssh_param "X11UseLocalhost" "yes" "$SSHD_CONFIG"
    update_ssh_param "AllowTcpForwarding" "yes" "$SSHD_CONFIG"
    
    # Set XAuthLocation if not already set
    XAUTH_PATH=$(which xauth)
    if [ -n "$XAUTH_PATH" ]; then
        if ! grep -q "^XAuthLocation" "$SSHD_CONFIG"; then
            echo "XAuthLocation $XAUTH_PATH" >> "$SSHD_CONFIG"
            print_info "Set XAuthLocation to: $XAUTH_PATH"
        fi
    fi
    
    print_success "SSH configuration updated for X11 forwarding"
}

verify_ssh_config() {
    print_header "Verifying SSH Configuration"
    
    # Test SSH config syntax
    if sshd -t 2>/dev/null; then
        print_success "SSH configuration syntax is valid"
    else
        print_error "SSH configuration has syntax errors"
        print_info "Running detailed test:"
        sshd -t
        exit 1
    fi
    
    # Display X11 related configuration
    print_info "Current X11 settings in sshd_config:"
    grep -E "^(X11Forwarding|X11DisplayOffset|X11UseLocalhost|AllowTcpForwarding|XAuthLocation)" "$SSHD_CONFIG" || print_warning "No X11 settings found"
}

restart_ssh_service() {
    if [ "$RESTART_SSH" = true ]; then
        print_header "Restarting SSH Service"
        
        if [ "$SERVICE_MANAGER" = "systemctl" ]; then
            systemctl restart sshd || systemctl restart ssh
            
            if systemctl is-active --quiet sshd || systemctl is-active --quiet ssh; then
                print_success "SSH service restarted successfully"
            else
                print_error "SSH service failed to restart"
                systemctl status sshd || systemctl status ssh
                exit 1
            fi
        elif [ "$SERVICE_MANAGER" = "rc-service" ]; then
            rc-service sshd restart
            
            if rc-service sshd status | grep -q "started"; then
                print_success "SSH service restarted successfully"
            else
                print_error "SSH service failed to restart"
                rc-service sshd status
                exit 1
            fi
        fi
    else
        print_warning "SSH service restart skipped (manual restart required)"
        if [ "$SERVICE_MANAGER" = "systemctl" ]; then
            print_info "Run: sudo systemctl restart sshd"
        else
            print_info "Run: sudo rc-service sshd restart"
        fi
    fi
}

setup_user_environment() {
    print_header "Setting Up User Environment"
    
    # Get current user (the one who invoked sudo)
    if [ -n "${SUDO_USER:-}" ]; then
        ACTUAL_USER=$SUDO_USER
        USER_HOME=$(getent passwd "$SUDO_USER" | cut -d: -f6)
    else
        ACTUAL_USER=$(whoami)
        USER_HOME=$HOME
    fi
    
    print_info "Configuring for user: $ACTUAL_USER"
    print_info "Home directory: $USER_HOME"
    
    # Create .Xauthority file if it doesn't exist
    XAUTH_FILE="$USER_HOME/.Xauthority"
    
    if [ ! -f "$XAUTH_FILE" ]; then
        sudo -u "$ACTUAL_USER" touch "$XAUTH_FILE"
        print_success "Created .Xauthority file"
    else
        print_info ".Xauthority file already exists"
    fi
    
    # Set proper permissions
    chown "$ACTUAL_USER":"$ACTUAL_USER" "$XAUTH_FILE"
    chmod 600 "$XAUTH_FILE"
    print_success "Set .Xauthority permissions (600)"
    
    # Verify
    ls -la "$XAUTH_FILE"
}

configure_firewall() {
    print_header "Checking Firewall Configuration"
    
    # Check if UFW is installed and active
    if command -v ufw &> /dev/null; then
        if ufw status | grep -q "Status: active"; then
            print_info "UFW firewall is active"
            
            # Check if SSH is allowed
            if ufw status | grep -q "22.*ALLOW"; then
                print_success "SSH (port 22) is allowed through firewall"
            else
                print_warning "SSH may not be allowed through firewall"
                # Only prompt if running interactively
                if [ -t 0 ]; then
                    read -p "Allow SSH through firewall? (Y/n) " -r
                    echo
                    if [[ ! $REPLY =~ ^[Nn]$ ]]; then
                        ufw allow OpenSSH
                        ufw reload
                        print_success "SSH allowed through firewall"
                    fi
                else
                    # Non-interactive mode: auto-allow SSH
                    print_info "Non-interactive mode: Allowing SSH through firewall"
                    ufw allow OpenSSH
                    ufw reload
                    print_success "SSH allowed through firewall"
                fi
            fi
        else
            print_info "UFW firewall is inactive"
        fi
    else
        print_info "UFW firewall not installed"
    fi
    
    # Check iptables
    if command -v iptables &> /dev/null; then
        if iptables -L -n | grep -q "22.*ACCEPT"; then
            print_success "SSH allowed in iptables"
        elif iptables -L -n | grep -q "ACCEPT.*all"; then
            print_info "iptables appears to allow all traffic"
        else
            print_warning "Check iptables rules manually if connection issues occur"
        fi
    fi
}

run_tests() {
    print_header "Running Verification Tests"
    
    # Test 1: Check if xauth is executable
    if command -v xauth &> /dev/null; then
        print_success "Test 1: xauth is available"
    else
        print_error "Test 1: xauth not found"
    fi
    
    # Test 2: Check SSH X11 forwarding settings
    if grep -q "^X11Forwarding yes" "$SSHD_CONFIG"; then
        print_success "Test 2: X11Forwarding enabled in sshd_config"
    else
        print_error "Test 2: X11Forwarding not enabled"
    fi
    
    # Test 3: Check SSH service status
    if systemctl is-active --quiet sshd || systemctl is-active --quiet ssh; then
        print_success "Test 3: SSH service is running"
    else
        print_error "Test 3: SSH service is not running"
    fi
    
    # Test 4: Check .Xauthority permissions
    if [ -n "${SUDO_USER:-}" ]; then
        USER_HOME=$(getent passwd "$SUDO_USER" | cut -d: -f6)
        XAUTH_FILE="$USER_HOME/.Xauthority"
        
        if [ -f "$XAUTH_FILE" ]; then
            PERMS=$(stat -c "%a" "$XAUTH_FILE")
            if [ "$PERMS" = "600" ]; then
