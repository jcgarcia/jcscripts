#!/bin/bash

# Restore script for docs backup from Dropbox
# Usage: restore-docs [destination]

# Configuration
DROPBOX_REMOTE="DropBox"
DROPBOX_PATH="/backups/docs-backup-latest.tar.gz"
DEFAULT_RESTORE_DIR="$HOME/docs-restored"
TEMP_ARCHIVE="/tmp/docs-backup-restore.tar.gz"

# Colors
RED='\033[0;31m'
GREEN='\033[0;32m'
YELLOW='\033[1;33m'
NC='\033[0m'

# Check if rclone is installed
if ! command -v rclone &> /dev/null; then
    echo -e "${RED}ERROR: rclone not installed${NC}"
    exit 1
fi

# Check if Dropbox remote is configured
if ! rclone listremotes | grep -q "^$DROPBOX_REMOTE:"; then
    echo -e "${RED}ERROR: $DROPBOX_REMOTE remote not configured${NC}"
    exit 1
fi

# Determine restore destination
RESTORE_DIR="${1:-$DEFAULT_RESTORE_DIR}"

echo -e "${GREEN}=== Docs Backup Restore ===${NC}"
echo ""
echo "Source: $DROPBOX_REMOTE:$DROPBOX_PATH"
echo "Destination: $RESTORE_DIR"
echo ""

# Ask for confirmation
read -p "Continue with restore? (y/N) " -n 1 -r
echo
if [[ ! $REPLY =~ ^[Yy]$ ]]; then
    echo -e "${YELLOW}Restore cancelled${NC}"
    exit 0
fi

# Download from Dropbox
echo -e "${YELLOW}Downloading backup from Dropbox...${NC}"
if ! rclone copy "$DROPBOX_REMOTE:$DROPBOX_PATH" /tmp/ -P; then
    echo -e "${RED}ERROR: Failed to download from Dropbox${NC}"
    exit 1
fi

# Rename downloaded file
mv /tmp/docs-backup-latest.tar.gz "$TEMP_ARCHIVE"

if [ ! -f "$TEMP_ARCHIVE" ]; then
    echo -e "${RED}ERROR: Archive not found after download${NC}"
    exit 1
fi

echo -e "${GREEN}✓ Download complete ($(du -h $TEMP_ARCHIVE | cut -f1))${NC}"

# Create restore directory
mkdir -p "$RESTORE_DIR"

# Extract archive
echo -e "${YELLOW}Extracting backup...${NC}"
if tar -xzf "$TEMP_ARCHIVE" -C "$RESTORE_DIR"; then
    echo -e "${GREEN}✓ Extraction complete${NC}"
else
    echo -e "${RED}ERROR: Failed to extract archive${NC}"
    rm -f "$TEMP_ARCHIVE"
    exit 1
fi

# Cleanup
rm -f "$TEMP_ARCHIVE"

echo ""
echo -e "${GREEN}=== Restore Complete ===${NC}"
echo "Files restored to: $RESTORE_DIR"
echo ""
echo "File count: $(find "$RESTORE_DIR" -type f | wc -l) files"
echo "Total size: $(du -sh "$RESTORE_DIR" | cut -f1)"
echo ""
echo "Next steps:"
echo "  • Review files: ls -la $RESTORE_DIR"
echo "  • Copy to original location: cp -r $RESTORE_DIR/* ~/docs/"
echo "  • Remove restored dir: rm -rf $RESTORE_DIR"

