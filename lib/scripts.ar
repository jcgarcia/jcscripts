VERSION="1.0"

# Display help message
show_help() {
  cat << EOF
Usage: awsinstall [OPTIONS]

Automatically detects your Linux distribution and installs AWS CLI v2 using
the recommended method for your system.

OPTIONS:
  -h, --help       Display this help message and exit
  -v, --version    Display script version and exit

DESCRIPTION:
  This script detects your Linux distribution and installs AWS CLI v2 using
  the official AWS installer (recommended) or the system package manager as
  a fallback.

SUPPORTED DISTRIBUTIONS:
  - Ubuntu/Debian (apt)
  - Fedora/Red Hat/CentOS (dnf/yum)
  - Arch Linux (pacman)
  - Alpine Linux (apk)

EXAMPLES:
  awsinstall              # Install AWS CLI on current system
  awsinstall -h           # Show this help message
  awsinstall -v           # Show version information

AUTHOR:
  Julio Cesar Garcia

EOF
}

# Function to extract the version from the script header of the current script
extract_version() {
    local script_file="$0"  # Referring to the current script
    grep -i '^# Version:' "$script_file" | awk -F': ' '{print $2}'
}

# Helper function to display the version
show_version() {
    version=$(extract_version)
    echo "awsinstall version: ${version}"
}

# Detect OS type
detect_os() {
  if [[ -f /etc/os-release ]]; then
    . /etc/os-release
    OS_ID="$ID"
    OS_VERSION="$VERSION_ID"
    OS_NAME="$NAME"
  elif [[ -f /etc/lsb-release ]]; then
    . /etc/lsb-release
    OS_ID="$DISTRIB_ID"
    OS_VERSION="$DISTRIB_RELEASE"
    OS_NAME="$DISTRIB_DESCRIPTION"
  else
    echo "ERROR: Unable to detect OS type."
    exit 1
  fi
  
  echo "Detected OS: $OS_NAME"
}

# Check if AWS CLI is already installed
check_existing_aws() {
  if command -v aws &> /dev/null; then
    local current_version=$(aws --version 2>&1)
    echo "AWS CLI is already installed: $current_version"
    read -p "Do you want to reinstall/upgrade AWS CLI? (y/n): " reinstall
    if [[ "$reinstall" != "y" && "$reinstall" != "Y" ]]; then
      echo "Installation cancelled."
      exit 0
    fi
  fi
}

# Install AWS CLI using official installer (recommended for all distros)
install_aws_official() {
  echo ""
  echo "Installing AWS CLI v2 using official AWS installer..."
  echo "This is the recommended method and works on all Linux distributions."
  echo ""
  
  # Create temporary directory
  local temp_dir=$(mktemp -d)
  cd "$temp_dir" || exit 1
  
  # Download AWS CLI
  echo "Downloading AWS CLI v2..."
  if ! curl -sSL "https://awscli.amazonaws.com/awscli-exe-linux-x86_64.zip" -o "awscliv2.zip"; then
    echo "ERROR: Failed to download AWS CLI."
    cd - > /dev/null
    rm -rf "$temp_dir"
    exit 1
  fi
  
  # Unzip the installer
  echo "Extracting AWS CLI installer..."
  if ! unzip -q awscliv2.zip; then
    echo "ERROR: Failed to extract AWS CLI installer."
    cd - > /dev/null
    rm -rf "$temp_dir"
    exit 1
  fi
  
  # Install AWS CLI
  echo "Installing AWS CLI (requires sudo)..."
  if sudo ./aws/install --update 2>/dev/null || sudo ./aws/install; then
    echo "AWS CLI installed successfully!"
  else
    echo "ERROR: AWS CLI installation failed."
    cd - > /dev/null
    rm -rf "$temp_dir"
    exit 1
  fi
  
  # Clean up
  cd - > /dev/null
  rm -rf "$temp_dir"
  
  # Verify installation
  if command -v aws &> /dev/null; then
    echo ""
    echo "✓ Installation verified: $(aws --version)"
    echo ""
  else
    echo "WARNING: AWS CLI installed but not found in PATH. You may need to restart your shell."
  fi
}

# Install AWS CLI using package manager (fallback)
install_aws_package_manager() {
  echo ""
  echo "Installing AWS CLI using system package manager..."
  echo "NOTE: This may install an older version. The official installer is recommended."
  echo ""
  
  case "$OS_ID" in
    ubuntu|debian)
      echo "Installing AWS CLI via apt..."
      sudo apt update
      sudo apt install -y awscli
      ;;
    fedora|rhel|centos|rocky|almalinux)
      echo "Installing AWS CLI via dnf/yum..."
      if command -v dnf &> /dev/null; then
        sudo dnf install -y awscli
      else
        sudo yum install -y awscli
      fi
      ;;
    arch|manjaro)
      echo "Installing AWS CLI via pacman..."
      sudo pacman -S --noconfirm aws-cli
      ;;
    alpine)
      echo "Installing AWS CLI via apk..."
      sudo apk add aws-cli
      ;;
    *)
      echo "ERROR: Unsupported distribution for package manager installation."
      echo "Please use the official installer method."
      exit 1
      ;;
  esac
  
  # Verify installation
  if command -v aws &> /dev/null; then
    echo ""
    echo "✓ AWS CLI installed: $(aws --version)"
    echo ""
  else
    echo "ERROR: AWS CLI installation failed."
    exit 1
  fi
}

# Main installation logic
main() {
  # Handle command-line options
  case "${1:-}" in
    -h|--help)
      show_help
      exit 0
      ;;
    -v|--version)
      show_version
      exit 0
      ;;
    -*)
      echo "ERROR: Unknown option: $1"
      echo "Use -h or --help for usage information."
      exit 1
      ;;
  esac
  
  echo "========================================"
  echo "   AWS CLI Installation Script"
  echo "========================================"
  echo ""
  
  # Detect OS
  detect_os
  
  # Check if AWS CLI is already installed
  check_existing_aws
  
  # Check for required tools (curl and unzip)
  local missing_tools=()
  command -v curl &> /dev/null || missing_tools+=("curl")
  command -v unzip &> /dev/null || missing_tools+=("unzip")
  
  if [[ ${#missing_tools[@]} -gt 0 ]]; then
    echo ""
    echo "WARNING: Missing required tools: ${missing_tools[*]}"
    echo "These tools are required for the official AWS installer."
    echo ""
    read -p "Install missing tools now? (y/n): " install_tools
    
    if [[ "$install_tools" == "y" || "$install_tools" == "Y" ]]; then
      case "$OS_ID" in
        ubuntu|debian)
          sudo apt update && sudo apt install -y "${missing_tools[@]}"
          ;;
        fedora|rhel|centos|rocky|almalinux)
          if command -v dnf &> /dev/null; then
            sudo dnf install -y "${missing_tools[@]}"
          else
            sudo yum install -y "${missing_tools[@]}"
          fi
          ;;
        arch|manjaro)
          sudo pacman -S --noconfirm "${missing_tools[@]}"
          ;;
        alpine)
          sudo apk add "${missing_tools[@]}"
          ;;
      esac
    fi
  fi
  
  # Ask user which method to use
  echo ""
  echo "Choose installation method:"
  echo "  1) Official AWS Installer (Recommended - Latest version)"
  echo "  2) System Package Manager (May be older version)"
  echo ""
  read -p "Enter choice [1-2] (default: 1): " choice
  choice=${choice:-1}
  
  case "$choice" in
    1)
      # Check again if curl and unzip are available
      if ! command -v curl &> /dev/null || ! command -v unzip &> /dev/null; then
        echo "ERROR: curl and unzip are required for official installer."
        echo "Falling back to package manager method..."
        install_aws_package_manager
      else
        install_aws_official
      fi
      ;;
    2)
      install_aws_package_manager
      ;;
    *)
      echo "ERROR: Invalid choice. Exiting."
      exit 1
      ;;
  esac
  
  echo "========================================"
  echo "   Installation Complete!"
  echo "========================================"
  echo ""
  echo "You can now use AWS CLI by running: aws --version"
  echo "To configure AWS CLI, run: aws configure"
  echo ""
}

# Run main function
main "$@"

