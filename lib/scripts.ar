    echo -e "${GREEN}✓${NC} $1"
}

print_error() {
    echo -e "${RED}✗${NC} $1"
}

print_warning() {
    echo -e "${YELLOW}⚠${NC} $1"
}

print_info() {
    echo -e "${BLUE}ℹ${NC} $1"
}

print_header() {
    echo -e "\n${BLUE}========================================${NC}"
    echo -e "${BLUE}$1${NC}"
    echo -e "${BLUE}========================================${NC}\n"
}

show_help() {
  cat << EOF
Usage: installbackup [OPTIONS]

Configure automated backups for directories with support for local storage,
NAS, and cloud providers (Dropbox via rclone).

OPTIONS:
  -h, --help       Display this help message and exit
  -v, --version    Display script version and exit

DESCRIPTION:
  This script sets up a complete backup solution including:
  
  - Backup scripts for each configured directory
  - Restore scripts for recovery
  - List and cleanup utilities
  - Cron jobs for automated daily backups
  - Anacron configuration for missed backups
  
  First run: Configure your first backup
  Subsequent runs: Add more backups or modify existing ones

FEATURES:
  • Multiple backup sources (docs, scripts, custom directories)
  • Local disk backups with versioning (keeps last 10)
  • Optional NAS synchronization
  • Optional cloud upload (Dropbox via rclone)
  • Automated scheduling via cron
  • Missed backup recovery via anacron
  • Manual backup/restore commands

EXAMPLES:
  installbackup              # Interactive setup
  installbackup -h           # Show this help
  
  After setup:
    mybackup                 # List configured backups
    mybackup docs            # Run backup-docs manually
    myrestore docs           # Restore from Dropbox
    listbackups              # View all available backups
    cleanbackups             # Remove empty/corrupted backups

AUTHOR:
  Julio Cesar Garcia

SEE ALSO:
  mybackup, myrestore, listbackups, cleanbackups

EOF
}

show_version() {
    echo "installbackup version $VERSION"
}

# ------------------------------------------------------------------------------
# Configuration Functions
# ------------------------------------------------------------------------------

load_config() {
    if [ -f "$CONFIG_FILE" ]; then
        source "$CONFIG_FILE"
    fi
}

save_config() {
    cat > "$CONFIG_FILE" << EOF
# Backup Configuration
# Generated by installbackup on $(date)

# Configured backups (comma-separated)
CONFIGURED_BACKUPS="$CONFIGURED_BACKUPS"
EOF
    
    # Save individual backup configurations
    for backup in $(echo "$CONFIGURED_BACKUPS" | tr ',' ' '); do
        local src_var="${backup}_SOURCE"
        local dest_var="${backup}_DEST"
        local nas_var="${backup}_NAS"
        local cloud_var="${backup}_CLOUD"
        local remote_var="${backup}_REMOTE"
        local schedule_var="${backup}_SCHEDULE"
        
        cat >> "$CONFIG_FILE" << EOF

# Configuration for $backup backup
${src_var}="${!src_var}"
${dest_var}="${!dest_var}"
${nas_var}="${!nas_var}"
${cloud_var}="${!cloud_var}"
${remote_var}="${!remote_var}"
${schedule_var}="${!schedule_var}"
EOF
    done
    
    print_success "Configuration saved to $CONFIG_FILE"
}

list_configured_backups() {
    if [ -z "$CONFIGURED_BACKUPS" ]; then
        echo "No backups configured yet."
        return 1
    fi
    
    print_header "Configured Backups"
    local count=1
    for backup in $(echo "$CONFIGURED_BACKUPS" | tr ',' ' '); do
        local src_var="${backup}_SOURCE"
        echo "$count. $backup → ${!src_var}"
        ((count++))
    done
    echo ""
    return 0
}

# ------------------------------------------------------------------------------
# Installation Functions
# ------------------------------------------------------------------------------

check_prerequisites() {
    print_header "Checking Prerequisites"
    
    # Check for required commands
    local missing=()
    
    for cmd in rsync tar cron; do
        if ! command -v $cmd &> /dev/null; then
            missing+=($cmd)
        fi
    done
    
    if [ ${#missing[@]} -gt 0 ]; then
        print_error "Missing required tools: ${missing[*]}"
        print_info "Install with: sudo apt install ${missing[*]}"
        return 1
    fi
    
    print_success "All required tools are installed"
    return 0
}

check_rclone() {
    if ! command -v rclone &> /dev/null; then
        print_warning "rclone not installed (required for cloud backups)"
        read -p "Install rclone now? (y/n): " -r
        if [[ $REPLY =~ ^[Yy]$ ]]; then
            print_info "Installing rclone..."
            curl https://rclone.org/install.sh | sudo bash
            if command -v rclone &> /dev/null; then
                print_success "rclone installed successfully"
                return 0
            else
                print_error "rclone installation failed"
                return 1
            fi
        else
            print_warning "Skipping rclone installation. Cloud backups will not be available."
            return 1
        fi
    else
        print_success "rclone is installed"
        return 0
    fi
}

setup_directories() {
    print_header "Setting Up Directories"
    
    mkdir -p "$JOBS_DIR"
    mkdir -p "$LOGS_DIR"
    
    print_success "Created $JOBS_DIR"
    print_success "Created $LOGS_DIR"
}

# ------------------------------------------------------------------------------
# Backup Configuration
# ------------------------------------------------------------------------------

configure_new_backup() {
    print_header "Configure New Backup"
    
    # Get backup name
    read -p "Backup name (e.g., docs, scripts, photos): " backup_name
    backup_name=$(echo "$backup_name" | tr '[:upper:]' '[:lower:]' | tr -d ' ')
    
    if [ -z "$backup_name" ]; then
        print_error "Backup name cannot be empty"
        return 1
    fi
    
    # Check if already configured
    if echo "$CONFIGURED_BACKUPS" | grep -q "$backup_name"; then
        print_warning "Backup '$backup_name' already configured"
        read -p "Reconfigure it? (y/n): " -r
        if [[ ! $REPLY =~ ^[Yy]$ ]]; then
            return 1
        fi
    fi
    
    # Get source directory
    read -p "Source directory (e.g., ~/docs): " source_dir
    source_dir="${source_dir/#\~/$HOME}"
    
    if [ ! -d "$source_dir" ]; then
        print_warning "Directory $source_dir does not exist"
        read -p "Create it? (y/n): " -r
        if [[ $REPLY =~ ^[Yy]$ ]]; then
            mkdir -p "$source_dir"
            print_success "Created $source_dir"
        else
            print_error "Cannot proceed without source directory"
            return 1
        fi
    fi
    
    # Get local destination
    read -p "Local backup destination: " local_dest
    local_dest="${local_dest/#\~/$HOME}"
    mkdir -p "$local_dest" 2>/dev/null || {
        print_error "Cannot create $local_dest"
        return 1
    }
    
    # Get NAS destination (optional)
    read -p "NAS destination (optional, press Enter to skip): " nas_dest
    nas_dest="${nas_dest/#\~/$HOME}"
    
    # Get cloud configuration (optional)
    cloud_enabled="false"
    cloud_remote=""
    read -p "Enable cloud backup? (y/n): " -r
    if [[ $REPLY =~ ^[Yy]$ ]]; then
        if check_rclone; then
            # List available remotes
            if rclone listremotes | grep -q ":"; then
                echo "Available rclone remotes:"
                rclone listremotes
                read -p "Enter remote name (e.g., DropBox): " cloud_remote
                cloud_remote=$(echo "$cloud_remote" | sed 's/:$//')
                
                if rclone listremotes | grep -q "^${cloud_remote}:"; then
                    cloud_enabled="true"
                    print_success "Cloud backup enabled with remote: $cloud_remote"
                else
                    print_error "Remote '$cloud_remote' not found"
                    print_info "Configure with: rclone config"
                fi
            else
                print_warning "No rclone remotes configured"
                print_info "Configure with: rclone config"
            fi
        fi
    fi
    
    # Get schedule
    read -p "Cron schedule time (default: 02:00, format HH:MM): " schedule_time
    schedule_time=${schedule_time:-02:00}
    
    # Validate time format
    if ! [[ $schedule_time =~ ^[0-2][0-9]:[0-5][0-9]$ ]]; then
        print_error "Invalid time format. Using default: 02:00"
        schedule_time="02:00"
    fi
    
    local hour=$(echo "$schedule_time" | cut -d: -f1)
    local minute=$(echo "$schedule_time" | cut -d: -f2)
    
    # Save configuration
    declare -g "${backup_name}_SOURCE=$source_dir"
    declare -g "${backup_name}_DEST=$local_dest"
    declare -g "${backup_name}_NAS=$nas_dest"
    declare -g "${backup_name}_CLOUD=$cloud_enabled"
    declare -g "${backup_name}_REMOTE=$cloud_remote"
    declare -g "${backup_name}_SCHEDULE=$minute $hour * * *"
    
    # Add to configured backups list
    if [ -z "$CONFIGURED_BACKUPS" ]; then
        CONFIGURED_BACKUPS="$backup_name"
    elif ! echo "$CONFIGURED_BACKUPS" | grep -q "$backup_name"; then
        CONFIGURED_BACKUPS="$CONFIGURED_BACKUPS,$backup_name"
    fi
    
    print_success "Backup configuration for '$backup_name' complete"
    
    # Create backup scripts
    create_backup_scripts "$backup_name"
    
    # Setup cron job
    setup_cron_job "$backup_name"
    
    # Setup anacron
    setup_anacron "$backup_name"
    
    return 0
}

# ------------------------------------------------------------------------------
# Script Generation
# ------------------------------------------------------------------------------

create_backup_scripts() {
    local name=$1
    local src_var="${name}_SOURCE"
    local dest_var="${name}_DEST"
    local nas_var="${name}_NAS"
    local cloud_var="${name}_CLOUD"
    local remote_var="${name}_REMOTE"
    
    print_info "Creating backup scripts for '$name'..."
    
    # Create backup script
    cat > "$JOBS_DIR/backup-$name" << 'EOBACKUP'
#!/bin/bash

# Backup script for BACKUP_NAME
# Generated by installbackup

SOURCE="SOURCE_DIR"
DEST="DEST_DIR"
NAS_DEST="NAS_DIR"
CLOUD_ENABLED="CLOUD_ENABLED"
CLOUD_REMOTE="CLOUD_REMOTE"
CLOUD_PATH="/backups/BACKUP_NAME-latest.tar.gz"

DATE=$(date +%Y-%m-%d_%H-%M-%S)
STARTDATE=$(date +%Y-%m-%d_%H-%M-%S)
LOGFILE="$HOME/logs/backup-BACKUP_NAME-$STARTDATE.log"

# Create destination folder
sudo mkdir -p "$DEST/$DATE"

# Perform backup
echo "Starting backup at $STARTDATE" >> "$LOGFILE"
sudo rsync -av --delete --no-perms --no-owner --no-group --chmod=ugo=rwX "$SOURCE/" "$DEST/$DATE/" >> "$LOGFILE" 2>&1

# Keep only last 10 backups
echo "Cleaning old backups..." >> "$LOGFILE"
sudo ls -dt "$DEST"/*/ 2>/dev/null | tail -n +11 | xargs -r sudo rm -rf >> "$LOGFILE" 2>&1

echo "Backup completed at $(date +%Y-%m-%d_%H-%M-%S)" >> "$LOGFILE"

# Sync to NAS if configured
if [ -n "$NAS_DEST" ] && [ -d "$NAS_DEST" ] || [ ! -d "$NAS_DEST" ]; then
    if [ -n "$NAS_DEST" ]; then
        echo "Syncing to NAS..." >> "$LOGFILE"
        sudo rsync -avh --progress "$DEST/" "$NAS_DEST/" >> "$LOGFILE" 2>&1
        echo "NAS sync completed at $(date +%Y-%m-%d_%H-%M-%S)" >> "$LOGFILE"
    fi
fi

# Upload to cloud if enabled
if [ "$CLOUD_ENABLED" = "true" ] && command -v rclone &> /dev/null; then
    echo "Uploading to cloud..." >> "$LOGFILE"
    
    TEMP_ARCHIVE="/tmp/BACKUP_NAME-backup-$DATE.tar.gz"
    sudo tar -czf "$TEMP_ARCHIVE" -C "$DEST/$DATE" . >> "$LOGFILE" 2>&1
    
    if [ -f "$TEMP_ARCHIVE" ]; then
        rclone copy "$TEMP_ARCHIVE" "$CLOUD_REMOTE:$(dirname $CLOUD_PATH)" >> "$LOGFILE" 2>&1
        rclone moveto "$CLOUD_REMOTE:$(dirname $CLOUD_PATH)/$(basename $TEMP_ARCHIVE)" \
                      "$CLOUD_REMOTE:$CLOUD_PATH" >> "$LOGFILE" 2>&1
        sudo rm -f "$TEMP_ARCHIVE"
        echo "Cloud upload completed at $(date +%Y-%m-%d_%H-%M-%S)" >> "$LOGFILE"
    fi
fi

echo "All backup operations completed at $(date +%Y-%m-%d_%H-%M-%S)" >> "$LOGFILE"
EOBACKUP

    # Replace placeholders
    sed -i "s|BACKUP_NAME|$name|g" "$JOBS_DIR/backup-$name"
    sed -i "s|SOURCE_DIR|${!src_var}|g" "$JOBS_DIR/backup-$name"
    sed -i "s|DEST_DIR|${!dest_var}|g" "$JOBS_DIR/backup-$name"
    sed -i "s|NAS_DIR|${!nas_var}|g" "$JOBS_DIR/backup-$name"
    sed -i "s|CLOUD_ENABLED|${!cloud_var}|g" "$JOBS_DIR/backup-$name"
    sed -i "s|CLOUD_REMOTE|${!remote_var}|g" "$JOBS_DIR/backup-$name"
    
    chmod +x "$JOBS_DIR/backup-$name"
    print_success "Created $JOBS_DIR/backup-$name"
    
    # Create restore script
    cat > "$JOBS_DIR/restore-$name" << 'EORESTORE'
#!/bin/bash

# Restore script for BACKUP_NAME
# Generated by installbackup

CLOUD_REMOTE="CLOUD_REMOTE"
CLOUD_PATH="/backups/BACKUP_NAME-latest.tar.gz"
DEFAULT_RESTORE_DIR="$HOME/BACKUP_NAME-restored"
TEMP_ARCHIVE="/tmp/BACKUP_NAME-restore.tar.gz"

RED='\033[0;31m'
GREEN='\033[0;32m'
YELLOW='\033[1;33m'
NC='\033[0m'

if ! command -v rclone &> /dev/null; then
    echo -e "${RED}ERROR: rclone not installed${NC}"
    exit 1
fi

if ! rclone listremotes | grep -q "^$CLOUD_REMOTE:"; then
    echo -e "${RED}ERROR: $CLOUD_REMOTE remote not configured${NC}"
    exit 1
fi

RESTORE_DIR="${1:-$DEFAULT_RESTORE_DIR}"

echo -e "${GREEN}=== BACKUP_NAME Backup Restore ===${NC}"
echo ""
echo "Source: $CLOUD_REMOTE:$CLOUD_PATH"
echo "Destination: $RESTORE_DIR"
echo ""

read -p "Continue with restore? (y/N) " -n 1 -r
echo
if [[ ! $REPLY =~ ^[Yy]$ ]]; then
    echo -e "${YELLOW}Restore cancelled${NC}"
    exit 0
fi

echo -e "${YELLOW}Downloading backup from cloud...${NC}"
rclone copy "$CLOUD_REMOTE:$CLOUD_PATH" /tmp/ -P || {
    echo -e "${RED}ERROR: Failed to download${NC}"
    exit 1
}

mv /tmp/BACKUP_NAME-latest.tar.gz "$TEMP_ARCHIVE"

if [ ! -f "$TEMP_ARCHIVE" ]; then
    echo -e "${RED}ERROR: Archive not found${NC}"
    exit 1
fi

echo -e "${GREEN}✓ Download complete ($(du -h $TEMP_ARCHIVE | cut -f1))${NC}"

mkdir -p "$RESTORE_DIR"

echo -e "${YELLOW}Extracting backup...${NC}"
tar -xzf "$TEMP_ARCHIVE" -C "$RESTORE_DIR" || {
    echo -e "${RED}ERROR: Extraction failed${NC}"
    rm -f "$TEMP_ARCHIVE"
    exit 1
}

rm -f "$TEMP_ARCHIVE"

echo ""
echo -e "${GREEN}=== Restore Complete ===${NC}"
echo "Files restored to: $RESTORE_DIR"
echo "File count: $(find "$RESTORE_DIR" -type f | wc -l) files"
echo "Total size: $(du -sh "$RESTORE_DIR" | cut -f1)"
EORESTORE

    # Replace placeholders
    sed -i "s|BACKUP_NAME|$name|g" "$JOBS_DIR/restore-$name"
    sed -i "s|CLOUD_REMOTE|${!remote_var}|g" "$JOBS_DIR/restore-$name"
    
    chmod +x "$JOBS_DIR/restore-$name"
    print_success "Created $JOBS_DIR/restore-$name"
}

create_shared_scripts() {
    # Only create these once
    if [ -f "$JOBS_DIR/list-backups" ]; then
        print_info "Shared scripts already exist"
        return 0
    fi
    
    print_info "Creating shared backup scripts..."
    
    # Copy existing scripts if they exist, otherwise create minimal versions
    if [ -f "$HOME/.jobs/list-backups" ]; then
        cp "$HOME/.jobs/list-backups" "$JOBS_DIR/list-backups"
    fi
    
    if [ -f "$HOME/.jobs/clean-empty-backups" ]; then
        cp "$HOME/.jobs/clean-empty-backups" "$JOBS_DIR/clean-empty-backups"
    fi
    
    chmod +x "$JOBS_DIR"/list-backups "$JOBS_DIR"/clean-empty-backups 2>/dev/null
    
    print_success "Shared scripts ready"
}

create_wrapper_scripts() {
    print_info "Creating wrapper scripts..."
    
    # Create mybackup
    cat > "$SCRIPTS_DIR/mybackup" << 'EOMYBACKUP'
#!/bin/bash

CONFIG_FILE="$HOME/.backup_config"

if [ ! -f "$CONFIG_FILE" ]; then
    echo "No backups configured. Run 'installbackup' first."
    exit 1
fi

source "$CONFIG_FILE"

show_help() {
    echo "Usage: mybackup [backup-name]"
    echo ""
    echo "Run manual backups for configured backup jobs"
    echo ""
    echo "Examples:"
    echo "  mybackup           # List available backups"
    echo "  mybackup docs      # Run backup-docs"
    echo "  mybackup -h        # Show this help"
}

if [ $# -eq 0 ] || [ "$1" = "-h" ] || [ "$1" = "--help" ]; then
    if [ "$1" = "-h" ] || [ "$1" = "--help" ]; then
        show_help
        exit 0
    fi
    
    echo "Available backups:"
    echo "================="
    for backup in $(echo "$CONFIGURED_BACKUPS" | tr ',' ' '); do
        echo "  $backup"
    done
    exit 0
fi

backup_name=$1

if ! echo "$CONFIGURED_BACKUPS" | tr ',' '\n' | grep -q "^$backup_name$"; then
    echo "ERROR: Backup '$backup_name' not configured"
    echo "Available: $(echo $CONFIGURED_BACKUPS | tr ',' ' ')"
    exit 1
fi

if [ ! -x "$HOME/.jobs/backup-$backup_name" ]; then
    echo "ERROR: Backup script not found: $HOME/.jobs/backup-$backup_name"
    exit 1
fi

echo "Running backup for: $backup_name"
"$HOME/.jobs/backup-$backup_name"
EOMYBACKUP

    chmod +x "$SCRIPTS_DIR/mybackup"
    print_success "Created mybackup command"
    
    # Create myrestore
    cat > "$SCRIPTS_DIR/myrestore" << 'EOMYRESTORE'
#!/bin/bash

CONFIG_FILE="$HOME/.backup_config"

if [ ! -f "$CONFIG_FILE" ]; then
    echo "No backups configured. Run 'installbackup' first."
    exit 1
fi

source "$CONFIG_FILE"

show_help() {
    echo "Usage: myrestore [backup-name] [destination]"
    echo ""
    echo "Restore backups from cloud storage"
    echo ""
    echo "Examples:"
    echo "  myrestore              # List available restores"
    echo "  myrestore docs         # Restore docs to ~/docs-restored"
    echo "  myrestore docs ~/docs  # Restore docs to ~/docs"
    echo "  myrestore -h           # Show this help"
}

if [ $# -eq 0 ] || [ "$1" = "-h" ] || [ "$1" = "--help" ]; then
    if [ "$1" = "-h" ] || [ "$1" = "--help" ]; then
        show_help
        exit 0
    fi
    
    echo "Available restores:"
    echo "=================="
    for backup in $(echo "$CONFIGURED_BACKUPS" | tr ',' ' '); do
        echo "  $backup"
    done
    exit 0
fi

backup_name=$1
restore_dest=$2

if ! echo "$CONFIGURED_BACKUPS" | tr ',' '\n' | grep -q "^$backup_name$"; then
    echo "ERROR: Backup '$backup_name' not configured"
    echo "Available: $(echo $CONFIGURED_BACKUPS | tr ',' ' ')"
    exit 1
fi

if [ ! -x "$HOME/.jobs/restore-$backup_name" ]; then
    echo "ERROR: Restore script not found: $HOME/.jobs/restore-$backup_name"
    exit 1
fi

echo "Running restore for: $backup_name"
if [ -n "$restore_dest" ]; then
    "$HOME/.jobs/restore-$backup_name" "$restore_dest"
else
    "$HOME/.jobs/restore-$backup_name"
fi
EOMYRESTORE

    chmod +x "$SCRIPTS_DIR/myrestore"
    print_success "Created myrestore command"
    
    # Create symlinks
    ln -sf "$JOBS_DIR/list-backups" "$SCRIPTS_DIR/listbackups" 2>/dev/null
    ln -sf "$JOBS_DIR/clean-empty-backups" "$SCRIPTS_DIR/cleanbackups" 2>/dev/null
    
    print_success "Created listbackups symlink"
    print_success "Created cleanbackups symlink"
}

# ------------------------------------------------------------------------------
# Cron & Anacron Setup
# ------------------------------------------------------------------------------

setup_cron_job() {
    local name=$1
    local schedule_var="${name}_SCHEDULE"
    local schedule="${!schedule_var}"
    
    print_info "Setting up cron job for '$name'..."
    
    # Check if cron job already exists
    if crontab -l 2>/dev/null | grep -q "backup-$name"; then
        print_warning "Cron job for '$name' already exists"
        read -p "Update it? (y/n): " -r
        if [[ ! $REPLY =~ ^[Yy]$ ]]; then
            return 0
        fi
        # Remove old entry
        crontab -l 2>/dev/null | grep -v "backup-$name" | crontab -
    fi
    
    # Add new cron job
    (crontab -l 2>/dev/null; echo "$schedule $JOBS_DIR/backup-$name >> $LOGS_DIR/backup-$name-cron.log 2>&1") | crontab -
    
    print_success "Cron job configured: $schedule"
}

setup_anacron() {
    local name=$1
    
    print_info "Setting up anacron for '$name'..."
    
    # Check if anacron is installed
    if ! command -v anacron &> /dev/null; then
        print_warning "anacron not installed"
        read -p "Install anacron? (y/n): " -r
        if [[ $REPLY =~ ^[Yy]$ ]]; then
            sudo apt install anacron -y || {
                print_error "Failed to install anacron"
                return 1
            }
        else
            print_warning "Skipping anacron setup"
            return 1
        fi
    fi
    
    # Create anacrontab entry
    local anacron_file="/etc/anacrontab"
    local entry="1	5	backup-$name	$JOBS_DIR/backup-$name"
    
    if sudo grep -q "backup-$name" "$anacron_file" 2>/dev/null; then
        print_info "Anacron entry for '$name' already exists"
    else
        echo "$entry" | sudo tee -a "$anacron_file" > /dev/null
        print_success "Anacron configured for missed backups"
    fi
}

# ------------------------------------------------------------------------------
# Main Function
# ------------------------------------------------------------------------------

main() {
    case "${1:-}" in
        -h|--help)
            show_help
            exit 0
            ;;
        -v|--version)
            show_version
            exit 0
            ;;
        -*)
            echo "ERROR: Unknown option: $1"
            echo "Use -h or --help for usage information."
            exit 1
            ;;
    esac
    
    print_header "Backup Installation & Configuration"
    
    # Check prerequisites
    check_prerequisites || exit 1
    
    # Setup directories
    setup_directories
    
    # Load existing configuration
    load_config
    
    # Show existing backups
    if list_configured_backups; then
        echo ""
        read -p "Add another backup? (y/n): " -r
        if [[ ! $REPLY =~ ^[Yy]$ ]]; then
            print_info "No changes made"
            exit 0
        fi
    fi
    
    # Configure new backup
    configure_new_backup || exit 1
    
    # Create shared scripts
    create_shared_scripts
    
    # Create wrapper scripts
    create_wrapper_scripts
    
    # Save configuration
    save_config
    
    print_header "Installation Complete!"
    
    echo "Configured backups:"
    for backup in $(echo "$CONFIGURED_BACKUPS" | tr ',' ' '); do
        echo "  ✓ $backup"
    done
    
    echo ""
    echo "Available commands:"
    echo "  mybackup              # List/run backups"
    echo "  myrestore             # List/restore from cloud"
    echo "  listbackups           # View all backups"
    echo "  cleanbackups          # Clean empty backups"
    echo ""
    echo "Backup scripts location: $JOBS_DIR"
    echo "Configuration file: $CONFIG_FILE"
    echo ""
}

main "$@"

