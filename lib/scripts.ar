  if ! command -v apt &> /dev/null; then
    echo "ERROR: apt package manager not found."
    return 1
  fi
  
  echo "Updating package lists..."
  sudo apt update
  
  echo "Installing surf browser..."
  sudo apt install -y surf
  
  if command -v surf &> /dev/null; then
    echo "✓ surf installed successfully!"
    echo "surf is a lightweight WebKit-based browser perfect for X11 forwarding."
    return 0
  else
    echo "ERROR: surf installation failed."
    return 1
  fi
