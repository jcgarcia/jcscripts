  echo "  ✓ Local files deleted"
  echo ""
  
  printcyan "To restore this project, use:"
  printgreen "  gitproject -e ${saved_private_org}/${saved_private_repo}"
  echo ""
  
  exit 0
}

# Function to extract/restore project from GitHub
extract_project() {
  local repo_spec="$1"
  
  printcyan "═══════════════════════════════════════════════════════════"
  printcyan "              EXTRACTING PROJECT FROM GITHUB"
  printcyan "═══════════════════════════════════════════════════════════"
  echo ""
  
  # Parse repo_spec (can be org/repo or just repo name)
  local org_name=""
  local repo_name=""
  
  if [[ "$repo_spec" == *"/"* ]]; then
    org_name="${repo_spec%%/*}"
    repo_name="${repo_spec##*/}"
  else
    org_name="$PRIVATE_ORG"
    repo_name="$repo_spec"
  fi
  
  printcyan "Repository: $org_name/$repo_name"
  echo ""
  
  # Check if repository exists on GitHub
  if ! gh repo view "${org_name}/${repo_name}" &>/dev/null; then
    printred "ERROR: Repository ${org_name}/${repo_name} not found on GitHub"
    exit 1
  fi
  
  # Clone the repository
  local current_dir="$(pwd)"
  local project_dir="$current_dir/$repo_name"
  
  if [[ -d "$project_dir" ]]; then
    printred "ERROR: Directory $project_dir already exists"
    exit 1
  fi
  
  printcyan "Cloning private repository..."
  
  if git clone "git@github.com:${org_name}/${repo_name}.git" "$project_dir"; then
    printgreen "✓ Repository cloned successfully"
  else
    printred "✗ Failed to clone repository"
    exit 1
  fi
  
  cd "$project_dir" || exit 1
  
  # Read .gitproject.conf if it exists
  local public_dir=""
  local public_repo=""
  local public_user=""
  
  if [[ -f ".gitproject.conf" ]]; then
    printcyan "Reading project configuration..."
    source ".gitproject.conf"
    public_dir="$PUBLIC_DIR"
    public_repo="$PUBLIC_REPO"
    public_user="${PUBLIC_USER:-$DEFAULT_PUBLIC_USER}"
    printgreen "✓ Configuration loaded"
  else
    printred "WARNING: No .gitproject.conf found. Using defaults."
    public_dir="public"
    public_repo="${repo_name%Proj}"
    public_user="$PUBLIC_USER"
  fi
  
  echo ""
  printcyan "Public repository setup:"
  echo "  Directory: $public_dir"
  echo "  GitHub: $public_user/$public_repo"
  echo ""
  
  # Check if public directory exists locally
  if [[ -d "$public_dir" ]]; then
    printcyan "Public directory found locally"
    
    # Check if it has a git repository
    if [[ ! -d "$public_dir/.git" ]]; then
      printcyan "Initializing git in public directory..."
      cd "$public_dir" || exit 1
      git init
      cd ..
    fi
  else
    printred "WARNING: Public directory $public_dir not found locally"
    read -p "Create empty public directory structure? (y/n): " create_public
    
    if [[ "$create_public" == "y" || "$create_public" == "Y" ]]; then
      printcyan "Creating public directory structure..."
      mkdir -p "$public_dir"
      cd "$public_dir" || exit 1
      git init
      
      # Create basic README
      cat > README.md << EOF
# $public_repo

Public repository for $repo_name

## Status

Extracted from GitHub - structure needs to be populated.
EOF
      
      git add README.md
      git commit -m "Initial commit after extraction"
      cd ..
      printgreen "✓ Public directory created"
    fi
  fi
  
  # Create or update public GitHub repository
  printcyan "Setting up public GitHub repository..."
  
  if gh repo view "${public_user}/${public_repo}" &>/dev/null; then
    printred "⚠ Public repository ${public_user}/${public_repo} already exists"
    read -p "Delete and recreate it? (y/n): " recreate
    
    if [[ "$recreate" == "y" || "$recreate" == "Y" ]]; then
      printcyan "Deleting existing public repository..."
      if gh repo delete "${public_user}/${public_repo}" --confirm 2>&1 | grep -q "deleted\|successfully"; then
        printgreen "✓ Existing repository deleted"
        
        printcyan "Creating new public repository..."
        if gh repo create "${public_user}/${public_repo}" --public --confirm 2>/dev/null; then
          printgreen "✓ Public repository created: ${public_user}/${public_repo}"
        else
          printred "✗ Failed to create public repository"
          exit 1
        fi
      else
        printred "✗ Failed to delete existing repository"
        exit 1
      fi
    else
      printcyan "Using existing repository"
    fi
  else
    if gh repo create "${public_user}/${public_repo}" --public --confirm 2>/dev/null; then
      printgreen "✓ Public repository created: ${public_user}/${public_repo}"
    else
      printred "✗ Failed to create public repository"
      exit 1
    fi
  fi
  
  # Connect public directory to GitHub
  if [[ -d "$public_dir" ]]; then
    cd "$public_dir" || exit 1
    
    # Add remote if not exists
    if ! git remote get-url origin &>/dev/null; then
      git remote add origin "git@github.com:${public_user}/${public_repo}.git"
      printgreen "✓ Remote added to public repository"
    fi
    
    # Push if there are commits
    if git rev-parse HEAD &>/dev/null; then
      printcyan "Pushing to public repository..."
      if git push -u origin main 2>/dev/null || git push -u origin master 2>/dev/null; then
        printgreen "✓ Pushed to public repository"
      else
        printred "✗ Failed to push (you may need to push manually)"
      fi
    fi
    
    cd ..
  fi
  
  echo ""
  printcyan "═══════════════════════════════════════════════════════════"
  printgreen "            PROJECT EXTRACTED SUCCESSFULLY!"
  printcyan "═══════════════════════════════════════════════════════════"
  echo ""
  
  printgreen "Project Location:    $project_dir"
  echo ""
  printcyan "Repositories:"
  printgreen "  Private:           git@github.com:${org_name}/${repo_name}.git"
  printgreen "  Public:            git@github.com:${public_user}/${public_repo}.git"
  echo ""
  
  printcyan "Next steps:"
  echo ""
  echo "  1. Navigate to project:"
  printgreen "     cd $project_dir"
  echo ""
  echo "  2. Review project structure"
  echo "  3. Update activity/status/current-context.md"
  echo ""
  
  exit 0
}

# Function to show advanced options documentation
show_advanced_help() {
  cat << 'EOF'
═══════════════════════════════════════════════════════════
           GITPROJECT - ADVANCED OPTIONS
═══════════════════════════════════════════════════════════

These are hidden/advanced options not shown in regular help.
Use with caution as some are destructive operations.

───────────────────────────────────────────────────────────
📦 EXTRACT - Restore Project from GitHub
───────────────────────────────────────────────────────────

Usage:
  gitproject -e <org>/<repo>
  gitproject --extract <org>/<repo>

Description:
  Restores a project from a private GitHub repository.
  This is useful for:
  - Recovering abandoned projects
  - Setting up project on a new machine
  - Restoring from backup

What it does:
  1. Clones the private repository from GitHub
  2. Reads .gitproject.conf for configuration
  3. Creates/connects the public repository
  4. Sets up GitHub remotes for both repos

Examples:
  # Extract project from organization
  gitproject -e Ingasti/MyAppProj
  
  # Extract using default organization (from config)
  gitproject -e MyAppProj

───────────────────────────────────────────────────────────
🗑️  ABANDON - Remove Project (DESTRUCTIVE)
───────────────────────────────────────────────────────────

Usage:
  gitproject -a <project-directory>
  gitproject --abandon <project-directory>

Description:
  Permanently removes project from local machine and deletes
  public GitHub repository while preserving private repo.
  
  ⚠️  THIS IS DESTRUCTIVE AND CANNOT BE UNDONE!

What it does:
  1. Deletes the public GitHub repository (remote)
  2. Keeps the private GitHub repository (backup)
  3. Deletes the entire local project directory

Requires:
  - Must type 'DELETE' to confirm
  - Project must have .gitproject.conf

Examples:
  # Abandon project in current location
  gitproject -a ~/projects/MyAppProj
  
  # Abandon project with relative path
  gitproject -a ./OldProject

Recovery:
  After abandoning, you can restore with:
  gitproject -e <org>/<repo>

───────────────────────────────────────────────────────────
📋 SHOW - Display This Help
───────────────────────────────────────────────────────────

Usage:
  gitproject -s
  gitproject --show

Description:
  Displays this advanced options documentation.

───────────────────────────────────────────────────────────
⚠️  SAFETY NOTES
───────────────────────────────────────────────────────────

1. ABANDON is destructive - always confirm you have the
   correct project before proceeding

2. Private repositories are preserved as backup when using
   ABANDON - you can always extract them later

3. EXTRACT will fail if local directory already exists -
   this prevents accidental overwrites

4. Always commit and push changes before using ABANDON

5. These options are hidden to prevent accidental use

───────────────────────────────────────────────────────────

For regular options, use:
  gitproject --help

═══════════════════════════════════════════════════════════
EOF
  exit 0
}

# Function to load configuration from ~/.jcscripts
load_configuration() {
  local config_file="$HOME/.jcscripts"
  local config_updated=false
  local file_created=false
  
  # Create config file if it doesn't exist
  if [[ ! -f "$config_file" ]]; then
    printcyan "Configuration file not found. Creating it..."
    echo ""
    
    # Create basic config file structure
    cat > "$config_file" << 'EOF'
# JC Scripts Configuration File
# This file stores default values for script configurations

EOF
    file_created=true
  fi
  
  # Always load existing config to check what's already there
  if [[ -f "$config_file" ]]; then
    source "$config_file"
  fi
  
  # Check and prompt for missing values
  if [[ -z "$DEFAULT_PRIVATE_ORG" ]]; then
    if [[ "$file_created" == false ]]; then
      echo ""
      printcyan "Git Configuration Setup - Missing Values"
      echo ""
    fi
    read -p "Enter your GitHub organization name (for private repos): " org_input
    if [[ -n "$org_input" ]]; then
      if [[ "$config_updated" == false ]]; then
        echo "" >> "$config_file"
        echo "# Git Configuration" >> "$config_file"
      fi
      echo "DEFAULT_PRIVATE_ORG=\"$org_input\"" >> "$config_file"
      DEFAULT_PRIVATE_ORG="$org_input"
      config_updated=true
    fi
  fi
  
  if [[ -z "$DEFAULT_PUBLIC_USER" ]]; then
    read -p "Enter your GitHub username (for public repos): " user_input
    if [[ -n "$user_input" ]]; then
      if [[ "$config_updated" == false ]]; then
        echo "" >> "$config_file"
        echo "# Git Configuration" >> "$config_file"
      fi
      echo "DEFAULT_PUBLIC_USER=\"$user_input\"" >> "$config_file"
      DEFAULT_PUBLIC_USER="$user_input"
      config_updated=true
    fi
  fi
  
  if [[ -z "$DEFAULT_AUTHOR_NAME" ]]; then
    read -p "Enter your full name (for commits/licenses): " name_input
    if [[ -n "$name_input" ]]; then
      if [[ "$config_updated" == false ]]; then
        echo "" >> "$config_file"
        echo "# Git Configuration" >> "$config_file"
      fi
      echo "DEFAULT_AUTHOR_NAME=\"$name_input\"" >> "$config_file"
      DEFAULT_AUTHOR_NAME="$name_input"
      config_updated=true
    fi
  fi
  
  if [[ -z "$DEFAULT_AUTHOR_EMAIL" ]]; then
    read -p "Enter your email (optional, press Enter to skip): " email_input
    if [[ -n "$email_input" ]]; then
      if [[ "$config_updated" == false ]]; then
        echo "" >> "$config_file"
        echo "# Git Configuration" >> "$config_file"
      fi
      echo "DEFAULT_AUTHOR_EMAIL=\"$email_input\"" >> "$config_file"
      DEFAULT_AUTHOR_EMAIL="$email_input"
      config_updated=true
    fi
  fi
  
  # Show success message if config was updated
  if [[ "$config_updated" == true ]]; then
    echo ""
    printgreen "✓ Configuration saved to $config_file"
    echo ""
  fi
  
  # Load values into working variables
  PRIVATE_ORG="${DEFAULT_PRIVATE_ORG}"
  PUBLIC_USER="${DEFAULT_PUBLIC_USER}"
  AUTHOR_NAME="${DEFAULT_AUTHOR_NAME}"
  AUTHOR_EMAIL="${DEFAULT_AUTHOR_EMAIL}"
  
  # Validate required config values
  if [[ -z "$PRIVATE_ORG" || -z "$PUBLIC_USER" || -z "$AUTHOR_NAME" ]]; then
    printred "ERROR: Missing required configuration values."
    echo ""
    echo "Required values:"
    echo "  - GitHub organization name"
    echo "  - GitHub username"
    echo "  - Author name"
    echo ""
    echo "Please run the script again to configure these values."
    exit 1
  fi
}

# Function to validate requirements
validate_requirements() {
  local missing_tools=()
  
  # Check for git
  if ! command -v git &> /dev/null; then
    missing_tools+=("git")
  fi
  
  # Check for gh CLI
  if ! command -v gh &> /dev/null; then
    missing_tools+=("gh (GitHub CLI)")
  fi
  
  if [[ ${#missing_tools[@]} -gt 0 ]]; then
    printred "ERROR: Missing required tools:"
    for tool in "${missing_tools[@]}"; do
      echo "  - $tool"
    done
    echo ""
    echo "Please install the missing tools and try again."
    return 1
  fi
  
  # Check GitHub authentication
  if ! gh auth status &> /dev/null; then
    printred "ERROR: GitHub CLI is not authenticated."
    echo "Please run: gh auth login"
    return 1
  fi
  
  return 0
}

# Function to extract version
extract_version() {
  local script_file="$0"
  grep -i '^# Version:' "$script_file" | awk -F': ' '{print $2}'
}

