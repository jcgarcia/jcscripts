# Function to update scripts from repository
update_scripts() {
  local force_update="$1"
  
  echo "=================================="
  echo "  Checking for Updates"
  echo "=================================="
  echo ""
  
  # Get repository URL from config
  local repo_url="https://raw.githubusercontent.com/jcgarcia/jcscripts/main"
  
  # Create temporary directory for checking updates
  local temp_dir=$(mktemp -d)
  
  echo "Checking for updates from repository..."
  echo ""
  
  # Download the latest installscripts to temp directory
  if curl -sSL "$repo_url/installscripts" -o "$temp_dir/installscripts" 2>/dev/null; then
    # Make it executable
    chmod +x "$temp_dir/installscripts"
    
    # Check if there are updates by comparing with local version
    if [[ -f "$SCRIPTS_DIR/installscripts" ]]; then
      local local_version=$(grep -i '^# Version:' "$SCRIPTS_DIR/installscripts" | awk -F': ' '{print $2}')
      local remote_version=$(grep -i '^# Version:' "$temp_dir/installscripts" | awk -F': ' '{print $2}')
      
      echo "Local version:  $local_version"
      echo "Remote version: $remote_version"
      echo ""
      
      if [[ "$local_version" == "$remote_version" && "$force_update" != "true" ]]; then
        echo "✓ Your scripts are up to date!"
        echo ""
        echo "Tip: Use 'scripts -f' to force reinstall even when versions match."
        rm -rf "$temp_dir"
        return 0
      elif [[ "$force_update" == "true" ]]; then
        echo "→ Force update requested - reinstalling scripts..."
        echo ""
      else
        echo "→ Updates available!"
        echo ""
        read -p "Do you want to update your scripts? (y/n): " update_confirm
        
        if [[ "$update_confirm" != "y" && "$update_confirm" != "Y" ]]; then
          echo "Update cancelled."
          rm -rf "$temp_dir"
          return 0
        fi
      fi
    else
      echo "→ installscripts not found locally. Proceeding with installation..."
      echo ""
    fi
    
    # Run the installscripts from temp directory
    echo "Running installation/update process..."
    echo ""
    "$temp_dir/installscripts"
    
    # Clean up
    rm -rf "$temp_dir"
    
    echo ""
    echo "=================================="
    echo "  Update Complete!"
    echo "=================================="
  else
    echo "ERROR: Unable to download updates from repository."
    echo "Please check your internet connection and try again."
    rm -rf "$temp_dir"
    return 1
  fi
}

# Function to list scripts
list_scripts() {
  local show_in_green="$1"
  local show_in_columns="$2"

  echo "List of available scripts"

  # Find all executable files in the scripts directory with no extension
  local scripts
  scripts=$(find "$SCRIPTS_DIR" -maxdepth 1 \( -type f -o -type l \) -executable ! -name ".*" ! -name "*.*")

  if [[ -z "$scripts" ]]; then
    printred "No scripts found in $SCRIPTS_DIR."
    return
  fi

  if [[ "$show_in_green" == "true" ]]; then
    # Short list: Collect script names, format into columns, and display in green
    echo "$scripts" | xargs -n1 basename | column -c $(tput cols) | while IFS= read -r line; do
      printgreen "$line"
    done
  else
    # Default: Display full information for each script and page the output with proper color handling
    while IFS= read -r script; do
      extract_header "$script"
    done <<< "$scripts" | less -R
  fi
}

# Main function
main() {
  local show_in_green="false"
  local show_in_columns="false"

  # Check if the scripts directory exists
  if [[ ! -d "$SCRIPTS_DIR" ]]; then
    echo "ERROR: Directory $SCRIPTS_DIR does not exist."
    exit 1
  fi

  # Use find to only target files and links in the specified directory (top-level only)
  local scripts
  scripts=$(find "$SCRIPTS_DIR" -maxdepth 1 \( -type f -o -type l \) ! -name "*.*" -exec file {} + | grep "ASCII text executable" | cut -d: -f1)

  if [[ -z "$scripts" ]]; then
    echo "No valid scripts found in $SCRIPTS_DIR."
    exit 0
  fi

  # Parse options
  while [[ $# -gt 0 ]]; do
    case "$1" in
      -s|--show)
        show_in_green="true"
        shift
        ;;
      -c|--columns)
        show_in_columns="true"
        shift
        ;;
      -h|--help)
        echo "Usage: scripts [options]"
        echo "Options:"
        echo "  -s | --show       Show scripts in green"
        echo "  -c | --columns    Display scripts in columns"
        echo "  -u | --update     Update scripts from repository"
        echo "  -f | --force      Force update even if version is the same"
        echo "  -d | --docs       Display documentation index"
        echo "  -h | --help       Show this help message"
        echo "  -v | --version    Show script version"
        exit 0
        ;;
      -v|--version)
        show_version
        exit 0
        ;;
      -u|--update)
        update_scripts "false"
        exit 0
        ;;
      -f|--force)
        update_scripts "true"
        exit 0
        ;;
      -d|--docs)
        show_documentation
        exit 0
        ;;
      *)
        printred "Invalid option: $1"
        exit 1
        ;;
    esac
  done

  # List scripts
  list_scripts "$show_in_green" "$show_in_columns"
}

# Execute the main function
main "$@"

