  done
  
  return 1  # No browser found
}

# Function to check if GUI is available
is_gui_available() {
  # Check if DISPLAY is set (X11 or Wayland session)
  if [[ -z "$DISPLAY" ]] && [[ -z "$WAYLAND_DISPLAY" ]]; then
    return 1  # No GUI environment
  fi
  
  # Test if the display actually works by checking X server connection
  # Only if we have tools to test it - otherwise assume it works
  if [[ -n "$DISPLAY" ]]; then
    if command -v xdpyinfo &>/dev/null; then
      if ! xdpyinfo &>/dev/null 2>&1; then
        return 1  # X server not accessible
      fi
    elif command -v xset &>/dev/null; then
      if ! xset q &>/dev/null 2>&1; then
        return 1  # X server not accessible
      fi
    fi
    # If no test tools available, assume DISPLAY is valid and proceed
  fi
  
  # Check if xdg-open is available
  if ! command -v xdg-open &>/dev/null; then
    return 1  # No xdg-open utility
  fi
  
  # Check if a browser is installed
  if ! check_browser_installed; then
    return 1  # No browser found
  fi
  
  return 0  # GUI is available with browser
}

# Function to handle SSO login
perform_sso_login() {
  # Check each requirement separately for clear messaging
  local has_display=false
  local has_xdg_open=false
  local has_browser=false
  local display_works=true  # Assume true unless proven otherwise
  
  # Check if DISPLAY is set
  if [[ -n "$DISPLAY" ]] || [[ -n "$WAYLAND_DISPLAY" ]]; then
    has_display=true
    
    # Test if X server is actually accessible (only if we have test tools)
    if [[ -n "$DISPLAY" ]]; then
      if command -v xdpyinfo &>/dev/null; then
        if ! xdpyinfo &>/dev/null 2>&1; then
          display_works=false
        fi
      elif command -v xset &>/dev/null; then
        if ! xset q &>/dev/null 2>&1; then
          display_works=false
        fi
      fi
      # If no test tools, assume display works
    fi
  fi
  
  if command -v xdg-open &>/dev/null; then
    has_xdg_open=true
  fi
  
  if check_browser_installed; then
    has_browser=true
  fi
  
  # Try automatic browser login if all requirements are met AND display works
  if $has_display && $display_works && $has_xdg_open && $has_browser; then
    # Quick X11 server test before attempting browser launch
    if timeout 3 bash -c "xdpyinfo &>/dev/null || xset q &>/dev/null" 2>/dev/null; then
      # X server is responding - AWS CLI will try browser, or show URL if it fails
      # This is the expected behavior for SSH sessions
      aws sso login --profile "$AWS_PROFILE"
      return
    else
      # X server not responding, use manual method
      echo "X11 server not responding. Using manual authentication method..."
      echo ""
      # Fall through to manual method below
    fi
  fi
  
  # Explain what's missing and provide guidance
  echo "Automatic browser login is not available on this system."
  echo ""
  
  if ! $has_browser; then
    echo "REASON: No web browser is installed."
    echo ""
    echo "SOLUTION: Install Firefox automatically:"
    echo "  • Run: ./browserinstall"
    echo ""
    echo "Then set up X11 forwarding for SSH sessions:"
    echo "  • Run: ./setupX11Forwarding"
    echo "  • Then reconnect with: ssh -X $(whoami)@$(hostname)"
    echo ""
  elif $has_display && ! $display_works; then
    echo "REASON: X11 forwarding is not working properly."
    echo ""
    echo "DISPLAY is set to: $DISPLAY"
    echo "But the X server is not accessible from this session."
    echo ""
    echo "SOLUTION: Reconnect with X11 forwarding enabled:"
    echo "  • From your LOCAL machine, run: ssh -X $(whoami)@$(hostname)"
    echo ""
    echo "If this doesn't work, verify X11 forwarding setup:"
    echo "  • On server: sudo ./setupX11Forwarding"
    echo "  • On client: Ensure you have an X server running (X.Org, XQuartz, VcXsrv, etc.)"
    echo ""
  elif ! $has_display; then
    echo "REASON: No GUI environment detected (headless server or SSH without X11)."
    echo ""
    echo "SOLUTION: Enable X11 forwarding for SSH sessions:"
    echo "  • On server: ./setupX11Forwarding"
    echo "  • Then reconnect: ssh -X $(whoami)@$(hostname)"
    echo ""
  elif ! $has_xdg_open; then
    echo "REASON: xdg-utils package not installed."
    echo ""
    echo "SOLUTION: Install xdg-utils:"
    echo "  • Ubuntu/Debian:  sudo apt install -y xdg-utils"
    echo "  • Fedora/RHEL:    sudo dnf install -y xdg-utils"
    echo "  • Arch Linux:     sudo pacman -S xdg-utils"
    echo ""
  fi
  
  echo "For now, using SSH tunnel method for authentication..."
  echo ""
  
  # Capture the full output first
  login_output=$(aws sso login --profile "$AWS_PROFILE" --no-browser 2>&1)
  
  # Extract the login URL (the actual https URL, not URL-encoded)
  login_url=$(echo "$login_output" | grep -Eo 'https://[^ ]+' | head -1)
  
  # Extract the callback port from the URL-encoded redirect_uri
  # Format: ...&redirect_uri=http%3A%2F%2F127.0.0.1%3A<PORT>%2F...
  callback_port=$(echo "$login_output" | grep -oP '(?<=127\.0\.0\.1%3A)[0-9]+(?=%2F)' | head -1)
  
  if [[ -z "$login_url" ]]; then
    echo "ERROR: Could not extract login URL from AWS SSO output."
    echo "Please try running: aws sso login --profile $AWS_PROFILE --no-browser"
    exit 1
  fi
  
  if [[ -z "$callback_port" ]]; then
    echo "WARNING: Could not detect callback port. You may need to check the AWS CLI output."
    callback_port="<PORT>"
  fi
  
  echo "========================================"
  echo "  AWS SSO Login - SSH Tunnel Method"
  echo "========================================"
  echo ""
  echo "To complete the login process over SSH, follow these steps:"
  echo ""
  echo "STEP 1: Create SSH tunnel from your LOCAL machine"
  echo "  Open a new terminal on your LOCAL computer and run:"
  echo "  ssh -L ${callback_port}:127.0.0.1:${callback_port} $(whoami)@$(hostname)"
  echo ""
  echo "STEP 2: Open this URL in your LOCAL browser:"
  echo "  ${login_url}"
  echo ""
  echo "STEP 3: Complete the authentication in your browser"
  echo "  - Enter your credentials"
  echo "  - Complete any MFA/2FA challenges"
  echo "  - The browser will redirect to localhost (through the tunnel)"
  echo ""
  echo "STEP 4: Return here - the script will detect completion automatically"
  echo "========================================"
  echo ""
  
  # Wait for the user to complete the login process with a timeout
  timeout=300  # 5 minutes
  elapsed=0
  interval=5
  
  echo "Waiting for login to complete..."
  while [[ $elapsed -lt $timeout ]]; do
    # Check if the token file exists and contains a valid token
    token_file=$(find ~/.aws/sso/cache/ -type f -name "*.json" 2>/dev/null | head -1)
    if [[ -n "$token_file" ]] && jq -e '.accessToken' "$token_file" &>/dev/null; then
      access_token=$(jq -r '.accessToken' "$token_file" 2>/dev/null)
      if [[ -n "$access_token" && "$access_token" != "null" ]]; then
        echo ""
        echo "✓ Login successful!"
        return
      fi
    fi
    
    # Show progress every 15 seconds
    if (( elapsed % 15 == 0 )); then
      echo "  Still waiting... ($elapsed/$timeout seconds)"
    fi
    
    sleep $interval
    elapsed=$((elapsed + interval))
  done
  
  echo ""
  echo "ERROR: Login process timed out after $timeout seconds."
  echo "Please try again and ensure:"
  echo "  1. SSH tunnel is created correctly"
  echo "  2. You open the URL in your browser"
  echo "  3. You complete the authentication"
  exit 1
}

