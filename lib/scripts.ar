#!/bin/bash

# ------------------------------------------------------------------------------
# Script Name:    awsfunctions
# Description:    Library of functions to manage AWS operations.
# Author:         Julio Cesar Garcia
# Date:           20/03/2025
# Version:        1.0
# ------------------------------------------------------------------------------

# Functions in this library:
# - export_aws_credentials: Export AWS credentials from credentials file to environment variables.
#
# Note: Additional AWS operations are implemented as standalone scripts for better modularity:
# - awslogin: Log in to AWS using SSO
# - awsaddprofile: Add a new AWS profile interactively
# - awsorg: Fetch and display AWS Organization layout
# - awsres: Dynamically retrieve AWS resource information
# - awsnewaccount: Create a new AWS account
# - awsmoveaccount: Move AWS accounts between organizational units
# - awsaccess: Manage AWS Identity Center operations
# - createawsaccount: Create AWS accounts
# - createOrgAdm: Create organization administrator profile
# - CreateOrganization: Create AWS organization
# - connecttoaws: Automate AWS connection processes
# - getamiinfo: Fetch and display information about AWS AMIs
# - listownamis: List owned AMIs for your AWS account

# Function to export AWS credentials from the credentials file
export_aws_credentials() {
    # Path to the AWS credentials file
    AWS_CREDENTIALS_FILE="$HOME/.aws/credentials"
    PROFILE="default" # Change this if you use a different profile

    # Check if the credentials file exists
    if [ ! -f "$AWS_CREDENTIALS_FILE" ]; then
        echo "AWS credentials file not found at $AWS_CREDENTIALS_FILE"
        return 1
    fi

    # Read credentials from the specified profile
    AWS_ACCESS_KEY_ID=$(awk -v profile="[$PROFILE]" '$0 == profile {getline; print $3}' "$AWS_CREDENTIALS_FILE")
    AWS_SECRET_ACCESS_KEY=$(awk -v profile="[$PROFILE]" '$0 == profile {getline; getline; print $3}' "$AWS_CREDENTIALS_FILE")
    AWS_SESSION_TOKEN=$(awk -v profile="[$PROFILE]" '$0 == profile {getline; getline; getline; print $3}' "$AWS_CREDENTIALS_FILE")

    # Check if credentials were successfully read
    if [ -z "$AWS_ACCESS_KEY_ID" ] || [ -z "$AWS_SECRET_ACCESS_KEY" ] || [ -z "$AWS_SESSION_TOKEN" ]; then
        echo "Failed to read AWS credentials for profile '$PROFILE'"
        return 1
    fi

    # Export the credentials as environment variables
    export AWS_ACCESS_KEY_ID
    export AWS_SECRET_ACCESS_KEY
    export AWS_SESSION_TOKEN

    echo "AWS credentials for profile '$PROFILE' have been exported."
    return 0
}

