#!/bin/bash

# List all available backups from local, NAS, and Dropbox
# Usage: list-backups [--local|--nas|--dropbox|--all]

# Configuration
LOCAL_DEST="/media/jcgarcia/F08290EB8290B794/backup/docs-backup"
NAS_DEST="/mnt/nas/docs-backup"
DROPBOX_REMOTE="DropBox"
DROPBOX_PATH="/backups"

# Colors
GREEN='\033[0;32m'
YELLOW='\033[1;33m'
BLUE='\033[0;34m'
CYAN='\033[0;36m'
NC='\033[0m'

# Default: show all
SHOW_LOCAL=true
SHOW_NAS=true
SHOW_DROPBOX=true

# Parse arguments
if [ $# -gt 0 ]; then
    SHOW_LOCAL=false
    SHOW_NAS=false
    SHOW_DROPBOX=false
    
    case "$1" in
        --local)
            SHOW_LOCAL=true
            ;;
        --nas)
            SHOW_NAS=true
            ;;
        --dropbox)
            SHOW_DROPBOX=true
            ;;
        --all)
            SHOW_LOCAL=true
            SHOW_NAS=true
            SHOW_DROPBOX=true
            ;;
        -h|--help)
            echo "Usage: list-backups [--local|--nas|--dropbox|--all]"
            echo ""
            echo "Options:"
            echo "  --local     Show only local disk backups"
            echo "  --nas       Show only NAS backups"
            echo "  --dropbox   Show only Dropbox backups"
            echo "  --all       Show all backups (default)"
            echo "  -h, --help  Show this help message"
            exit 0
            ;;
        *)
            echo "Unknown option: $1"
            echo "Use -h or --help for usage information"
            exit 1
            ;;
    esac
fi

echo -e "${GREEN}=== Backup Locations ===${NC}"
echo ""

# Function to list directory backups
list_dir_backups() {
    local location=$1
    local path=$2
    local count=0
    local total_size=0
    
    echo -e "${BLUE}📁 $location${NC}"
    echo "Path: $path"
    
    if [ ! -d "$path" ]; then
        echo -e "${YELLOW}  ⚠ Directory not accessible${NC}"
        echo ""
        return
    fi
    
    echo ""
    printf "%-30s %10s %20s\n" "BACKUP NAME" "SIZE" "DATE"
    echo "────────────────────────────────────────────────────────────────"
    
    # List backups sorted by date (newest first)
    while IFS= read -r backup; do
        if [ -n "$backup" ]; then
            local name=$(basename "$backup")
            local size=$(du -sh "$backup" 2>/dev/null | cut -f1)
            local date=$(stat -c %y "$backup" 2>/dev/null | cut -d' ' -f1,2 | cut -d':' -f1,2)
            
            printf "%-30s %10s %20s\n" "$name" "$size" "$date"
            ((count++))
        fi
    done < <(sudo ls -dt "$path"/*/ 2>/dev/null)
    
    if [ $count -eq 0 ]; then
        echo "  No backups found"
    else
        total_size=$(sudo du -sh "$path" 2>/dev/null | cut -f1)
        echo "────────────────────────────────────────────────────────────────"
        echo -e "${CYAN}Total: $count backups, $total_size${NC}"
    fi
    
    echo ""
}

# Function to list Dropbox backups
list_dropbox_backups() {
    echo -e "${BLUE}☁️  Dropbox${NC}"
    echo "Remote: $DROPBOX_REMOTE:$DROPBOX_PATH"
    
    if ! command -v rclone &> /dev/null; then
        echo -e "${YELLOW}  ⚠ rclone not installed${NC}"
        echo ""
        return
    fi
    
    if ! rclone listremotes | grep -q "^$DROPBOX_REMOTE:"; then
        echo -e "${YELLOW}  ⚠ $DROPBOX_REMOTE remote not configured${NC}"
        echo ""
        return
    fi
    
    echo ""
    printf "%-40s %10s %20s\n" "FILE NAME" "SIZE" "MODIFIED"
    echo "────────────────────────────────────────────────────────────────────────"
    
    local count=0
    while IFS= read -r line; do
        if [ -n "$line" ]; then
            # Parse rclone ls output: size filename
            local size=$(echo "$line" | awk '{print $1}')
            local name=$(echo "$line" | awk '{$1=""; print $0}' | xargs)
            
            # Convert size to human readable
            if [ $size -lt 1024 ]; then
                size="${size}B"
            elif [ $size -lt 1048576 ]; then
                size="$((size / 1024))K"
            elif [ $size -lt 1073741824 ]; then
                size="$((size / 1048576))M"
            else
                size="$((size / 1073741824))G"
            fi
            
            # Get file modification time
            local modified=$(rclone lsl "$DROPBOX_REMOTE:$DROPBOX_PATH/$name" 2>/dev/null | awk '{print $2, $3}')
            
            printf "%-40s %10s %20s\n" "$name" "$size" "$modified"
            ((count++))
        fi
    done < <(rclone ls "$DROPBOX_REMOTE:$DROPBOX_PATH" 2>/dev/null)
    
    if [ $count -eq 0 ]; then
        echo "  No backups found"
    else
        echo "────────────────────────────────────────────────────────────────────────"
        echo -e "${CYAN}Total: $count file(s)${NC}"
    fi
    
    echo ""
}

# Show backups based on flags
if [ "$SHOW_LOCAL" = true ]; then
    list_dir_backups "Local Disk" "$LOCAL_DEST"
fi

if [ "$SHOW_NAS" = true ]; then
    list_dir_backups "NAS" "$NAS_DEST"
fi

if [ "$SHOW_DROPBOX" = true ]; then
    list_dropbox_backups
fi

echo -e "${GREEN}=== Summary ===${NC}"
echo "Use restore-docs to restore from Dropbox backup"
echo "Use rsync to restore from local or NAS backups"
echo ""
