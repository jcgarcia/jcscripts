# Function to parse arguments
parse_arguments() {
  # Check for special flags first (config, help, version)
  if [[ $# -eq 0 ]]; then
    printred "ERROR: Project name is required"
    echo ""
    show_help
    exit 1
  fi
  
  # Handle special flags that don't require project name
  case "$1" in
    -c|--config|--configure)
      manage_configuration
      ;;
    -h|--help)
      show_help
      exit 0
      ;;
    -v|--version)
      show_version
      exit 0
      ;;
    -s|--show)
      show_advanced_help
      ;;
    -r|--refactor)
      REFACTOR_MODE="true"
      if [[ -z "$2" ]]; then
        printred "ERROR: --refactor requires a project directory"
        echo ""
        show_help
        exit 1
      fi
      PROJECT_NAME="$2"
      shift 2
      return
      ;;
    -a|--abandon)
      ABANDON_MODE="true"
      if [[ -z "$2" ]]; then
        printred "ERROR: --abandon requires a project directory"
        exit 1
      fi
      PROJECT_NAME="$2"
      shift 2
      return
      ;;
    -e|--extract)
      EXTRACT_MODE="true"
      if [[ -z "$2" ]]; then
        printred "ERROR: --extract requires a repository specification (org/repo)"
        exit 1
      fi
      PROJECT_NAME="$2"
      shift 2
      return
      ;;
  esac
  
  PROJECT_NAME="$1"
  shift
  
  while [[ $# -gt 0 ]]; do
    case "$1" in
      -t|--type)
        PROJECT_TYPE="$2"
        shift 2
        ;;
      -m|--monorepo)
        CREATE_MONOREPO="true"
        shift
        ;;
      --components)
        COMPONENTS="$2"
        shift 2
        ;;
      -p|--private)
        PRIVATE_ONLY="true"
        CREATE_PUBLIC_REPO="false"
        shift
        ;;
      -o|--org)
        PRIVATE_ORG="$2"
        shift 2
        ;;
      -u|--user)
        PUBLIC_USER="$2"
        shift 2
        ;;
      --no-public)
        CREATE_PUBLIC_REPO="false"
        shift
        ;;
      --no-activity)
        CREATE_ACTIVITY="false"
        shift
        ;;
      -d|--description)
        PROJECT_DESCRIPTION="$2"
        shift 2
        ;;
      -r|--refactor)
        # Already handled in the first case statement
        shift
        ;;
      -c|--config|--configure)
        manage_configuration
        ;;
      -h|--help)
        show_help
        exit 0
        ;;
      -v|--version)
        show_version
        exit 0
        ;;
      *)
        printred "ERROR: Unknown option: $1"
        echo ""
        show_help
        exit 1
        ;;
    esac
  done
  
  # Set default description if not provided
  if [[ -z "$PROJECT_DESCRIPTION" ]]; then
    PROJECT_DESCRIPTION="$PROJECT_NAME project"
  fi
}

# Main function
main() {
  # Parse arguments
  parse_arguments "$@"
  
  # Load configuration
  load_configuration
  
  # Handle abandon mode
  if [[ "$ABANDON_MODE" == "true" ]]; then
    # Convert to absolute path if relative
    local project_path="$PROJECT_NAME"
    if [[ ! "$project_path" = /* ]]; then
      project_path="$(pwd)/$project_path"
    fi
    
    abandon_project "$project_path"
    exit 0
  fi
  
  # Handle extract mode
  if [[ "$EXTRACT_MODE" == "true" ]]; then
    extract_project "$PROJECT_NAME"
    exit 0
  fi
  
  # Handle refactor mode
  if [[ "$REFACTOR_MODE" == "true" ]]; then
    # Convert to absolute path if relative
    local project_path="$PROJECT_NAME"
    if [[ ! "$project_path" = /* ]]; then
      project_path="$(pwd)/$project_path"
    fi
    
    refactor_project "$project_path"
    exit 0
  fi
  
  # Validate requirements
  if ! validate_requirements; then
    exit 1
  fi
  
  # Determine project directory and public repo name
  local current_dir="$(pwd)"
  local project_dir="$current_dir/$PROJECT_NAME"
  local public_name
  
  # Remove "Proj" suffix if present for public repo name
  if [[ "$PROJECT_NAME" == *"Proj" ]]; then
    public_name="${PROJECT_NAME%Proj}"
  else
    public_name="$PROJECT_NAME"
  fi
  
  # Check if directory already exists
  if [[ -d "$project_dir" ]]; then
    printred "ERROR: Directory $project_dir already exists"
    exit 1
  fi
  
  # Preview and confirm
  if ! preview_project "$project_dir" "$public_name"; then
    exit 0
  fi
  
  echo ""
  printcyan "Starting project creation..."
  echo ""
  
  # Create project
  create_directory_structure "$project_dir"
  create_templates "$project_dir"
  setup_monorepo "$project_dir" "$public_name"
  initialize_git "$project_dir" "$public_name"
  create_initial_files "$project_dir" "$public_name"
  
  # Display summary
  display_summary "$project_dir" "$public_name"
}

# Execute main function
main "$@"

