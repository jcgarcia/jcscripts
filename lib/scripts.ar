#!/bin/bash

# ------------------------------------------------------------------------------
# Script Name:    ocires
# Description:    Retrieve and display Oracle OCI resource information using the OCI CLI config file.
# Usage:          ocires [-h|--help] [-v|--version] [-r|--resources] [-a|--all]
# Options:        -h | --help        Show this help message
#                 -v | --version     Show script version
#                 -r | --resources   Fetch and display key OCI resources
#                 -a | --all         Fetch all resources via structured search
# Author:         Julio Cesar Garcia
# Date:           09/04/2025
# Version:        2.0
# ------------------------------------------------------------------------------

# Display usage information
show_help() {
  echo "Usage: ./ocires [options]"
  echo "Options:"
  echo "  -h | --help        Show this help message"
  echo "  -v | --version     Show script version"
  echo "  -r | --resources   Fetch and display key OCI resources"
  echo "  -a | --all         Fetch all resources via structured search"
}

# Display script version
show_version() {
  echo "ocires version 2.0"
}

# Error handling function
handle_error() {
  local exit_code=$1
  local message=$2
  echo "Error: $message (Exit Code: $exit_code)"
  exit $exit_code
}

# Validate if OCI CLI is installed and properly configured
validate_cli() {
  if ! command -v oci &> /dev/null; then
    handle_error 1 "OCI CLI is not installed. Please install it and try again."
  fi
}

# Function to fetch and display key OCI resources
oci_resources() {
  echo "Listing key resources using the OCI CLI configuration..."
  echo "-----------------------------------------------"

  # List compute instances
  echo "Compute Instances:"
  oci compute instance list --profile DEFAULT --output table
  echo "-----------------------------------------------"

  # List block volumes
  echo "Block Volumes:"
  oci bv volume list --profile DEFAULT --output table
  echo "-----------------------------------------------"

  # List VCNs
  echo "Virtual Cloud Networks (VCNs):"
  oci network vcn list --profile DEFAULT --output table
  echo "-----------------------------------------------"

  # List buckets
  echo "Object Storage Buckets:"
  oci os bucket list --profile DEFAULT --output table
  echo "-----------------------------------------------"
}

# Function to fetch all OCI resources via structured search
oci_all_resources() {
  echo "Fetching all resources using the OCI CLI configuration..."
  echo "-----------------------------------------------"

  oci search resource structured-search \
    --query "query all resources" \
    --profile DEFAULT --output table
  echo "-----------------------------------------------"
}

# Main script logic
main() {
  # Validate CLI setup
  validate_cli

  # Check if no arguments were provided
  if [ $# -eq 0 ]; then
    show_help
    exit 0
  fi

  # Parse command-line options
  while [[ $# -gt 0 ]]; do
    case "$1" in
      -h|--help)
        show_help
        exit 0
        ;;
      -v|--version)
        show_version
        exit 0
        ;;
      -r|--resources)
        oci_resources
        exit 0
        ;;
      -a|--all)
        oci_all_resources
        exit 0
        ;;
      *)
        echo "Unknown option: $1"
        show_help
        exit 1
        ;;
    esac
    shift
  done
}

# Execute the main function
main "$@"

