  printred "═══════════════════════════════════════════════════════════"
  echo ""
  
  # Check if project directory exists
  if [[ ! -d "$project_dir" ]]; then
    printred "ERROR: Directory $project_dir does not exist"
    exit 1
  fi
  
  cd "$project_dir" || exit 1
  
  # Check if it's a git repository
  if [[ ! -d ".git" ]]; then
    printred "ERROR: $project_dir is not a git repository"
    exit 1
  fi
  
  # Try to read .gitproject.conf
  if [[ ! -f ".gitproject.conf" ]]; then
    printred "ERROR: No .gitproject.conf found. Cannot determine repository configuration."
    exit 1
  fi
  
  source ".gitproject.conf"
  
  # Save repository info before we delete everything
  local saved_public_user="$PUBLIC_USER"
  local saved_public_repo="$PUBLIC_REPO"
  local saved_private_org="$PRIVATE_ORG"
  local saved_private_repo="$PRIVATE_REPO"
  
  # Check if .gitproject.conf is committed to private repo
  printcyan "Checking if configuration is saved to private repository..."
  
  if ! git ls-files --error-unmatch .gitproject.conf &>/dev/null; then
    printcyan "Configuration not yet committed. Committing to private repo..."
    git add .gitproject.conf
    git commit -m "Save project configuration before abandonment" -o .gitproject.conf
    
    if git push origin main 2>/dev/null || git push origin master 2>/dev/null; then
      printgreen "✓ Configuration saved to private repository"
    else
      printred "✗ Failed to push configuration to private repository"
      printred "  Extraction may not work without this configuration"
      read -p "Continue anyway? (y/n): " continue_confirm
      if [[ "$continue_confirm" != "y" && "$continue_confirm" != "Y" ]]; then
        echo ""
        printgreen "Abandon cancelled."
