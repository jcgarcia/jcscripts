    exit 0
  fi
  
  echo ""
  printcyan "Starting abandon operation..."
  echo ""
  
  # Delete public GitHub repository
  if [[ -n "$saved_public_repo" && -n "$saved_public_user" ]]; then
    printcyan "Deleting public GitHub repository..."
    
    # First check if repo exists
    if gh repo view "${saved_public_user}/${saved_public_repo}" &>/dev/null; then
      # Use --confirm flag instead of --yes (--yes is deprecated)
      if gh repo delete "${saved_public_user}/${saved_public_repo}" --confirm 2>&1 | grep -q "deleted\|successfully"; then
