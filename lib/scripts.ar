RESET=$(tput sgr0 2>/dev/null || echo "")

# Functions for printing messages in specific colors
printred() {
  echo -e "${BOLD}${RED}$1${RESET}" | sed 's/\x1b(B//g'
}

printgreen() {
  echo -e "${BOLD}${GREEN}$1${RESET}" | sed 's/\x1b(B//g'
}

printyellow() {
  echo -e "${BOLD}${YELLOW}$1${RESET}" | sed 's/\x1b(B//g'
