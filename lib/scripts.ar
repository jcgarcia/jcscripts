  if command -v dnf &> /dev/null; then
    pkg_manager="dnf"
  elif command -v yum &> /dev/null; then
    pkg_manager="yum"
  else
    echo "ERROR: Neither dnf nor yum package manager found."
    return 1
  fi
  
  echo "Installing surf using $pkg_manager..."
  sudo $pkg_manager install -y surf
  
  if command -v surf &> /dev/null; then
    echo "✓ surf installed successfully!"
    echo "surf is a lightweight WebKit-based browser perfect for X11 forwarding."
