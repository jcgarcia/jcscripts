VERSION="1.0"

# Colors for output
RED='\033[0;31m'
GREEN='\033[0;32m'
YELLOW='\033[1;33m'
BLUE='\033[0;34m'
NC='\033[0m' # No Color

# ------------------------------------------------------------------------------
# Helper Functions
# ------------------------------------------------------------------------------

print_success() {
    echo -e "${GREEN}✓${NC} $1"
}

print_error() {
    echo -e "${RED}✗${NC} $1"
}

print_warning() {
    echo -e "${YELLOW}⚠${NC} $1"
}

print_info() {
    echo -e "${BLUE}ℹ${NC} $1"
}

print_header() {
    echo -e "\n${BLUE}========================================${NC}"
    echo -e "${BLUE}$1${NC}"
    echo -e "${BLUE}========================================${NC}\n"
}

show_help() {
  cat << EOF
Usage: X11reset [OPTIONS] [hostname]

Reset X11 forwarding by closing SSH ControlMaster connections and verifying
X11 forwarding is working properly.

ARGUMENTS:
  hostname         The SSH host to reset (optional if -a is used)

OPTIONS:
  -h, --help       Display this help message and exit
  -v, --version    Display script version and exit
  -a, --all        Reset all ControlMaster connections

DESCRIPTION:
  This script resolves X11 forwarding issues by:
  - Closing SSH ControlMaster connections that may be reusing old sessions
  - Verifying X11 forwarding is configured in SSH config
  - Testing the new connection has DISPLAY set properly
  - Providing troubleshooting information

COMMON ISSUES RESOLVED:
  - DISPLAY variable is empty despite ForwardX11 yes in config
  - X11 applications fail with "Can't open display"
  - SSH reusing old connection from before X11 was enabled on server

EXAMPLES:
  X11reset factory              # Reset X11 forwarding for factory host
  X11reset -a                   # Reset all ControlMaster connections
  X11reset factory && ssh factory 'xclock'  # Reset and test

REQUIREMENTS:
  - SSH ControlMaster configured (Host * with ControlMaster auto)
  - ForwardX11 yes in SSH client config
  - X11Forwarding yes on remote server

AUTHOR:
  Julio Cesar Garcia

SEE ALSO:
  setupX11Forwarding - Configure X11 forwarding on servers
  browserinstall - Install browsers for X11 forwarding

EOF
}

show_version() {
    echo "X11reset version: ${VERSION}"
}

# ------------------------------------------------------------------------------
# Main Functions
# ------------------------------------------------------------------------------

reset_controlmaster() {
    local host=$1
    
    print_info "Checking ControlMaster connection for: $host"
    
    # Check if ControlMaster is running
    if ssh -O check "$host" 2>&1 | grep -q "Master running"; then
        print_warning "ControlMaster connection found"
        print_info "Closing ControlMaster connection..."
        
        if ssh -O exit "$host" 2>&1 | grep -q "Exit request sent"; then
            print_success "ControlMaster connection closed"
        else
            print_error "Failed to close ControlMaster connection"
            return 1
        fi
    else
        print_info "No active ControlMaster connection found"
    fi
    
    return 0
}

reset_all_controlmasters() {
    print_header "Resetting All ControlMaster Connections"
    
    local cm_dir="$HOME/.ssh"
    local cm_pattern="cm-*"
    local found=false
    
    # Find all ControlMaster socket files
    if [ -d "$cm_dir" ]; then
        while IFS= read -r -d '' socket; do
            found=true
            local basename=$(basename "$socket")
            # Extract hostname from socket name (format: cm-user@host:port)
            local host_info=$(echo "$basename" | sed 's/^cm-//')
            
            print_info "Found ControlMaster socket: $basename"
            
            # Try to close it by extracting the host
            local host=$(echo "$host_info" | cut -d@ -f2 | cut -d: -f1)
            if [ -n "$host" ]; then
                ssh -O exit "$host" 2>/dev/null && print_success "Closed connection to $host" || print_warning "Could not close $host"
            fi
            
            # Remove the socket file if it still exists
            if [ -S "$socket" ]; then
                rm -f "$socket" 2>/dev/null && print_success "Removed socket: $basename"
            fi
        done < <(find "$cm_dir" -name "$cm_pattern" -type s -print0 2>/dev/null)
    fi
    
    if [ "$found" = false ]; then
        print_info "No ControlMaster connections found"
    fi
}

verify_x11_forwarding() {
    local host=$1
    
    print_header "Verifying X11 Forwarding"
    
    # Check SSH client config
    print_info "Checking SSH client configuration for: $host"
    
    if grep -A 20 "^Host $host" ~/.ssh/config 2>/dev/null | grep -q "ForwardX11.*yes"; then
        print_success "ForwardX11 enabled in SSH config"
    else
        print_warning "ForwardX11 not found in SSH config for $host"
        print_info "Add to ~/.ssh/config:"
        cat << EOF
    Host $host
        ForwardX11 yes
        ForwardX11Trusted yes
EOF
    fi
    
    # Test connection and check DISPLAY
    print_info "Testing X11 forwarding on new connection..."
    
    local display=$(ssh "$host" 'echo $DISPLAY' 2>&1)
    
    if [ -n "$display" ] && [[ "$display" =~ localhost:[0-9]+ ]]; then
        print_success "X11 forwarding is working! DISPLAY=$display"
        
        # Suggest testing with an X11 app
        print_info "Test with: ssh $host 'xeyes' or 'xclock'"
        return 0
    else
        print_error "X11 forwarding not working. DISPLAY is: ${display:-empty}"
        print_warning "Troubleshooting steps:"
        echo "  1. Ensure X11Forwarding yes on server: ssh $host 'grep ^X11Forwarding /etc/ssh/sshd_config'"
        echo "  2. Check xauth on server: ssh $host 'command -v xauth'"
        echo "  3. Verify SSH config: grep -A 10 'Host $host' ~/.ssh/config"
        echo "  4. Run setupX11Forwarding on the server"
        echo "  5. Try: ssh -X $host (force X11 forwarding)"
        return 1
    fi
}

# ------------------------------------------------------------------------------
# Main Execution
# ------------------------------------------------------------------------------

main() {
    local reset_all=false
    local host=""
    
    # Parse arguments
    while [[ $# -gt 0 ]]; do
        case $1 in
            -h|--help)
                show_help
                exit 0
                ;;
            -v|--version)
                show_version
                exit 0
                ;;
            -a|--all)
                reset_all=true
                shift
                ;;
            -*)
                echo "ERROR: Unknown option: $1"
                echo "Use -h or --help for usage information."
                exit 1
                ;;
            *)
                host=$1
                shift
                ;;
        esac
    done
    
    print_header "X11 Forwarding Reset Tool"
    
    if [ "$reset_all" = true ]; then
        reset_all_controlmasters
    elif [ -n "$host" ]; then
        reset_controlmaster "$host"
        verify_x11_forwarding "$host"
    else
        print_error "No hostname provided"
        echo ""
        echo "Usage: X11reset [hostname]"
        echo "       X11reset -a           # Reset all connections"
        echo "       X11reset -h           # Show help"
        exit 1
    fi
    
    echo ""
    print_success "X11 reset complete!"
    
    if [ -n "$host" ]; then
        echo ""
        print_info "Next steps:"
        echo "  • Connect with: ssh $host"
        echo "  • Test X11: ssh $host 'xeyes'"
        echo "  • View docs: ssh $host 'scripts -d'"
    fi
}

