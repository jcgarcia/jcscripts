#!/bin/bash

# ------------------------------------------------------------------------------
# Script Name:    awslogin
# Description:    Dynamically sets AWS default profile after SSO login.
#                 Supports remote SSH sessions with automatic tunnel instructions.
# Usage:          awslogin
# Options:
#                 -h | --help       Show this help message
#                 -v | --version    Show script version
# Author:         Julio Cesar Garcia
# Date:           2025-11-20
# Version:        4.3
# ------------------------------------------------------------------------------
CONFIG_FILE="$HOME/.scripts/.jcscripts"
SCRIPTS_DIR="$HOME/.scripts"
AWS_CONFIG_FILE="$HOME/.aws/config"

# Load the functions file 
source "$SCRIPTS_DIR/awsfunctions"

# Function to check and create AWS config if it doesn't exist
check_aws_config() {
  if [[ ! -f "$AWS_CONFIG_FILE" ]]; then
    echo "AWS config file not found at $AWS_CONFIG_FILE"
    echo "Creating minimal AWS SSO configuration..."
    
    # Create .aws directory if it doesn't exist
    mkdir -p "$HOME/.aws"
    
    # Prompt for SSO configuration
    read -p "Enter SSO start URL (e.g., https://your-org.awsapps.com/start): " sso_start_url
    read -p "Enter SSO region (e.g., us-east-1): " sso_region
    
    # Validate inputs
    if [[ -z "$sso_start_url" ]]; then
      echo "ERROR: SSO start URL is required."
      exit 1
    fi
    
    if [[ -z "$sso_region" ]]; then
      echo "ERROR: SSO region is required."
      exit 1
    fi
    
    # Create the config file with minimal SSO configuration
    cat > "$AWS_CONFIG_FILE" << EOF
[default]
sso_start_url = ${sso_start_url}
sso_region = ${sso_region}
sso_registration_scopes = sso:account:access
region = ${sso_region}
output = json
