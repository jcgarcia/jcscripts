#!/bin/bash

# ------------------------------------------------------------------------------
# Script Name:    scripts
# Description:    Lists all available scripts in the .scripts directory.
# Usage:          scripts [-s | -c | -u | -f | -d]
# Options:
#                 -s | --show       Show scripts in green
#                 -c | --columns    Display scripts in columns
#                 -u | --update     Update scripts from repository
#                 -f | --force      Force update even if version is the same
#                 -d | --docs       Display documentation index
# Author:         Julio Cesar Garcia
# Date:           20/03/2025
# Version:        5.3
# ------------------------------------------------------------------------------

# Functions in this library:
# - extract_header: Extract and display header information from a script.
# - list_short: Display script names in columns.
# - extract_version: Extract the version from the script header.
# - show_version: Display the version of the current script.
# - list_scripts: List all valid scripts in the directory.
# - install_glow: Install glow markdown renderer if not present.
# - show_documentation: Display documentation index using glow.
# - main: Main function to parse options and execute the script.

# Source the colorfunctions library
source "$HOME/.scripts/colorfunctions"

# Load the configuration file from the home directory or the .scripts directory
CONFIG_FILE="$HOME/.jcscripts"
if [[ -f "$CONFIG_FILE" ]]; then
  source "$CONFIG_FILE"
  # Expand ~ to $HOME in SCRIPTS_DIR if needed
  SCRIPTS_DIR="${SCRIPTS_DIR/#\~/$HOME}"
  EDITOR="${EDITOR:-vi}" # Ensure EDITOR has a fallback
elif [[ -f "$HOME/.scripts/.jcscripts" ]]; then
  CONFIG_FILE="$HOME/.scripts/.jcscripts"
  source "$CONFIG_FILE"
  # Expand ~ to $HOME in SCRIPTS_DIR if needed
  SCRIPTS_DIR="${SCRIPTS_DIR/#\~/$HOME}"
  EDITOR="${EDITOR:-vi}" # Ensure EDITOR has a fallback
else
  echo "Configuration file not found in $HOME or $HOME/.scripts. Using default values."
  SCRIPTS_DIR="$HOME/.scripts"
  EDITOR="vi"
fi

# Function to extract and display header information
extract_header() {
  local script="$1"
  printgreen "$(basename "$script")"
  echo "--------------------------------------"

  # Extract labeled header fields and print them in cyan
  grep -E "^(# *(Script Name|Description|Usage|Author|Date|Version):)" "$script" \
    | sed -e 's/^# *//' \
          -e '/\$script_name/d' \
          -e '/\$LOGNAME/d' \
          -e '/\$(date/d' \
    | while IFS= read -r line; do
        printcyan "$line"
      done

  echo
}

# Function to list script names in columns
list_short() {
  local scripts="$1"

  # Extract only the script names, ensuring single arguments per basename
  for script in $scripts; do
    basename "$script"
  done | column
}

# Function to extract the version from the script header of the current script
extract_version() {
    local script_file="$0"  # Referring to the current script
    grep -i '^# Version:' "$script_file" | awk -F': ' '{print $2}'
}

# Helper function to display the version
show_version() {
    version=$(extract_version)
    echo -e "${BOLD}${CYAN}Version: ${version}${RESET}" 
}

# Function to install glow if not present
install_glow() {
  echo "Glow markdown renderer is not installed."
  echo ""
  read -p "Would you like to install glow now? (y/n): " install_confirm
  
  if [[ "$install_confirm" != "y" && "$install_confirm" != "Y" ]]; then
    echo "Installation cancelled. You can install glow manually:"
    echo "  • Ubuntu/Debian:  sudo mkdir -p /etc/apt/keyrings && wget -qO- https://repo.charm.sh/apt/gpg.key | sudo gpg --dearmor -o /etc/apt/keyrings/charm.gpg && echo 'deb [signed-by=/etc/apt/keyrings/charm.gpg] https://repo.charm.sh/apt/ * *' | sudo tee /etc/apt/sources.list.d/charm.list && sudo apt update && sudo apt install glow"
    echo "  • Fedora/RHEL:    sudo yum-config-manager --add-repo https://repo.charm.sh/yum/ && sudo yum install glow"
    echo "  • Arch:           sudo pacman -S glow"
    echo "  • Homebrew:       brew install glow"
    echo "  • Go:             go install github.com/charmbracelet/glow@latest"
    return 1
  fi
  
  echo ""
  echo "Installing glow..."
  
  # Detect OS
  if [[ -f /etc/os-release ]]; then
    . /etc/os-release
    OS_ID="$ID"
  else
    echo "ERROR: Unable to detect OS type."
    return 1
  fi
  
  case "$OS_ID" in
    ubuntu|debian|linuxmint|pop)
      echo "Setting up Charm repository for Debian/Ubuntu..."
      sudo mkdir -p /etc/apt/keyrings
      wget -qO- https://repo.charm.sh/apt/gpg.key | sudo gpg --dearmor -o /etc/apt/keyrings/charm.gpg
      echo "deb [signed-by=/etc/apt/keyrings/charm.gpg] https://repo.charm.sh/apt/ * *" | sudo tee /etc/apt/sources.list.d/charm.list
      sudo apt update
      sudo apt install -y glow
      ;;
    fedora|rhel|centos|rocky|almalinux)
      echo "Setting up Charm repository for Fedora/RHEL..."
      cat <<EOF | sudo tee /etc/yum.repos.d/charm.repo
[charm]
name=Charm
baseurl=https://repo.charm.sh/yum/
enabled=1
gpgcheck=1
gpgkey=https://repo.charm.sh/yum/gpg.key
EOF
      sudo yum install -y glow
      ;;
    arch|manjaro|endeavouros)
      echo "Installing glow via pacman..."
      sudo pacman -S --noconfirm glow
      ;;
    alpine)
      echo "Installing glow from GitHub releases (not available in Alpine repos)..."
      # Detect architecture
      ARCH=$(uname -m)
      case "$ARCH" in
        x86_64)
          GLOW_ARCH="x86_64"
          ;;
        aarch64)
          GLOW_ARCH="arm64"
          ;;
        armv7l)
          GLOW_ARCH="armv7"
          ;;
        *)
          echo "ERROR: Unsupported architecture: $ARCH"
          echo "Please install glow manually: https://github.com/charmbracelet/glow/releases"
          return 1
          ;;
      esac
      
      # Get latest version
      GLOW_VERSION=$(wget -qO- https://api.github.com/repos/charmbracelet/glow/releases/latest | grep '"tag_name"' | sed -E 's/.*"v([^"]+)".*/\1/')
      if [[ -z "$GLOW_VERSION" ]]; then
        echo "ERROR: Could not determine latest glow version"
        return 1
      fi
      
      echo "Downloading glow v$GLOW_VERSION for $GLOW_ARCH..."
      GLOW_URL="https://github.com/charmbracelet/glow/releases/download/v${GLOW_VERSION}/glow_${GLOW_VERSION}_Linux_${GLOW_ARCH}.tar.gz"
      
      # Download and extract
      wget -q "$GLOW_URL" -O /tmp/glow.tar.gz || {
        echo "ERROR: Failed to download glow"
        return 1
      }
      
      # Extract to temp directory
      mkdir -p /tmp/glow_install
      tar -xzf /tmp/glow.tar.gz -C /tmp/glow_install || {
        echo "ERROR: Failed to extract glow"
        rm -rf /tmp/glow_install /tmp/glow.tar.gz
        return 1
      }
      
      # Find and install the glow binary (it's in a subdirectory named glow_VERSION_Linux_ARCH/)
      GLOW_BIN=$(find /tmp/glow_install -name "glow" -type f -executable 2>/dev/null | head -1)
      
      if [[ -n "$GLOW_BIN" && -f "$GLOW_BIN" ]]; then
        sudo cp "$GLOW_BIN" /usr/local/bin/glow
        sudo chmod +x /usr/local/bin/glow
        echo "Glow installed to /usr/local/bin/glow"
      else
        echo "ERROR: glow binary not found in archive"
        rm -rf /tmp/glow_install /tmp/glow.tar.gz
        return 1
      fi
      
      # Cleanup
      rm -rf /tmp/glow_install /tmp/glow.tar.gz
      ;;
    *)
      echo "Unsupported distribution: $OS_ID"
      echo "Please install glow manually: https://github.com/charmbracelet/glow"
      return 1
      ;;
  esac
  
  # Verify installation
  if command -v glow &> /dev/null; then
    echo ""
    printgreen "✓ Glow installed successfully!"
    return 0
  else
    echo ""
    printred "✗ Glow installation failed."
    return 1
  fi
}

# Function to install MarkText if not present
install_marktext() {
  echo "MarkText markdown viewer is not installed."
  echo ""
  read -p "Would you like to install MarkText now? (y/n): " install_confirm
  
  if [[ "$install_confirm" != "y" && "$install_confirm" != "Y" ]]; then
    echo "Installation cancelled. Falling back to terminal viewer."
    return 1
  fi
  
  echo ""
  echo "Installing MarkText..."
  
  # Detect OS
  if [[ -f /etc/os-release ]]; then
    . /etc/os-release
    OS_ID="$ID"
  else
    echo "ERROR: Unable to detect OS type."
    return 1
  fi
  
  case "$OS_ID" in
    ubuntu|debian|linuxmint|pop)
      echo "Downloading MarkText .deb package..."
      local temp_dir=$(mktemp -d)
      cd "$temp_dir"
      wget https://github.com/marktext/marktext/releases/download/v0.17.1/marktext-amd64.deb
      sudo dpkg -i marktext-amd64.deb
      sudo apt-get install -f -y
      cd - > /dev/null
      rm -rf "$temp_dir"
      ;;
    fedora|rhel|centos|rocky|almalinux)
      echo "Downloading MarkText .rpm package..."
      local temp_dir=$(mktemp -d)
      cd "$temp_dir"
      wget https://github.com/marktext/marktext/releases/download/v0.17.1/marktext-x86_64.rpm
      sudo rpm -i marktext-x86_64.rpm
      cd - > /dev/null
      rm -rf "$temp_dir"
      ;;
    arch|manjaro|endeavouros)
      echo "Installing MarkText via AUR..."
      if command -v yay &> /dev/null; then
        yay -S --noconfirm marktext-bin
      elif command -v paru &> /dev/null; then
        paru -S --noconfirm marktext-bin
      else
        echo "Please install an AUR helper (yay or paru) first, or install manually from AUR."
        return 1
      fi
      ;;
    *)
      echo "Unsupported distribution: $OS_ID"
      echo "Please install MarkText manually: https://github.com/marktext/marktext/releases"
      return 1
      ;;
  esac
  
  # Verify installation
  if command -v marktext &> /dev/null; then
    echo ""
    printgreen "✓ MarkText installed successfully!"
    return 0
  else
    echo ""
    printred "✗ MarkText installation failed."
    return 1
  fi
}

# Function to install grip
install_grip() {
  echo "Installing grip..."
  
  # Method 1: Try pipx first (recommended for Python apps on modern systems)
  if command -v pipx &> /dev/null; then
    echo "Using pipx..."
    if pipx install grip; then
      # Ensure PATH is updated for pipx
      pipx ensurepath >/dev/null 2>&1
      # Add to current session
      [[ ":$PATH:" != *":$HOME/.local/bin:"* ]] && export PATH="$HOME/.local/bin:$PATH"
      return 0
    fi
  fi
  
  # Method 2: Check if pipx is available to install
  if command -v apt &> /dev/null; then
    echo "Installing pipx first..."
    if sudo apt install -y pipx 2>/dev/null; then
      pipx install grip
      pipx ensurepath >/dev/null 2>&1
      [[ ":$PATH:" != *":$HOME/.local/bin:"* ]] && export PATH="$HOME/.local/bin:$PATH"
      return 0
    fi
  elif command -v dnf &> /dev/null; then
    echo "Installing pipx first..."
    if sudo dnf install -y pipx 2>/dev/null; then
      pipx install grip
      pipx ensurepath >/dev/null 2>&1
      [[ ":$PATH:" != *":$HOME/.local/bin:"* ]] && export PATH="$HOME/.local/bin:$PATH"
      return 0
    fi
  elif command -v pacman &> /dev/null; then
    if sudo pacman -S --noconfirm python-pipx 2>/dev/null; then
      pipx install grip
      pipx ensurepath >/dev/null 2>&1
      [[ ":$PATH:" != *":$HOME/.local/bin:"* ]] && export PATH="$HOME/.local/bin:$PATH"
      return 0
    fi
  fi
  
  # Method 3: Try pip with --user in a way that works with externally-managed environments
  if command -v pip3 &> /dev/null; then
    echo "Trying pip3 with virtual environment workaround..."
    # Some systems allow --user even with external management
    if pip3 install --user grip 2>/dev/null; then
      [[ ":$PATH:" != *":$HOME/.local/bin:"* ]] && export PATH="$HOME/.local/bin:$PATH"
      return 0
    fi
  fi
  
  # Method 4: Legacy pip
  if command -v pip &> /dev/null; then
    if pip install --user grip 2>/dev/null; then
      [[ ":$PATH:" != *":$HOME/.local/bin:"* ]] && export PATH="$HOME/.local/bin:$PATH"
      return 0
    fi
  fi
  
  # All methods failed
  echo ""
  printyellow "Unable to install grip automatically."
  printyellow "Please install grip manually using one of these methods:"
  echo "  • pipx install grip             (Recommended - run: apt install pipx first)"
  echo "  • dnf install python3-grip      (Fedora/RHEL)"
  echo "  • pacman -S python-grip         (Arch)"
  echo ""
  
  return 1
}

# Function to display documentation
show_documentation() {
  local docs_index="$SCRIPTS_DIR/docs/INDEX.md"
  local docs_dir="$SCRIPTS_DIR/docs"
  
  # Check if INDEX exists
  if [[ ! -f "$docs_index" ]]; then
    printred "ERROR: Documentation index not found at $docs_index"
    exit 1
  fi
  
  # Detect environment: prefer terminal viewer for SSH sessions without X11 forwarding
  # If X11 forwarding is active, prefer Firefox/Chrome over surf/netsurf
  local use_terminal=true
  local use_surf=false
  local browser_cmd=""
  local real_browser=""
  
  # Check if we're in an SSH session with X11 forwarding
  if [[ -n "$SSH_CONNECTION" ]] || [[ -n "$SSH_CLIENT" ]] || [[ -n "$SSH_TTY" ]]; then
    # We're in SSH - check if X11 forwarding is active
    if [[ -n "$DISPLAY" ]]; then
      # First check for real browsers (Firefox, Chrome, etc.)
      # Include common snap and flatpak paths
      for browser in firefox /snap/bin/firefox firefox-esr chromium chromium-browser google-chrome; do
        if command -v "$browser" &> /dev/null || [[ -x "$browser" ]]; then
          real_browser="$browser"
          browser_cmd="$browser"
          use_terminal=false
          use_surf=false
          break
        fi
      done
      
      # If no real browser found, fall back to surf/netsurf
      if [[ -z "$real_browser" ]]; then
        if command -v surf &> /dev/null; then
          # X11 forwarding is active and surf is available
          browser_cmd="surf"
          use_surf=true
          use_terminal=false
        elif command -v netsurf &> /dev/null; then
          # X11 forwarding is active and netsurf is available
          browser_cmd="netsurf"
          use_surf=true
          use_terminal=false
        fi
      fi
    fi
  # Check if we're on a local desktop
  elif [[ -n "$DISPLAY" ]] || [[ -n "$WAYLAND_DISPLAY" ]]; then
    # Local desktop environment
    use_terminal=false
  fi
  
  # Terminal mode - use glow
  if [[ "$use_terminal" == "true" ]]; then
    if ! command -v glow &> /dev/null; then
      install_glow || exit 1
    fi
    
    if command -v glow &> /dev/null; then
      cd "$docs_dir"
      # Set TERM to ensure proper rendering
      export TERM=${TERM:-xterm-256color}
      glow -s dark INDEX.md | less -R
    else
      echo "Displaying documentation with less..."
      less "$docs_index"
    fi
    return 0
  fi
  
  # SSH with X11 forwarding - use detected browser
  if [[ "$use_surf" == "true" ]] || [[ -n "$real_browser" ]]; then
    # If we have a real browser, use grip with it
    if [[ -n "$real_browser" ]]; then
      printgreen "Opening documentation with $real_browser (X11 forwarding detected)..."
      echo "Tip: Use browser's navigation buttons and links to browse documentation"
      
      # Start grip server and open with real browser
      local grip_cmd=""
      if command -v grip &> /dev/null; then
        grip_cmd="grip"
      elif [[ -x "$HOME/.local/bin/grip" ]]; then
        grip_cmd="$HOME/.local/bin/grip"
        [[ ":$PATH:" != *":$HOME/.local/bin:"* ]] && export PATH="$HOME/.local/bin:$PATH"
      fi
      
      # If grip not found, try to install it
      if [[ -z "$grip_cmd" ]]; then
        printyellow "Grip not installed. Attempting to install..."
        if install_grip; then
          if command -v grip &> /dev/null; then
            grip_cmd="grip"
          elif [[ -x "$HOME/.local/bin/grip" ]]; then
            grip_cmd="$HOME/.local/bin/grip"
            [[ ":$PATH:" != *":$HOME/.local/bin:"* ]] && export PATH="$HOME/.local/bin:$PATH"
          fi
        fi
      fi
      
      if [[ -n "$grip_cmd" ]]; then
        # Check if grip server is already running
        if lsof -ti:6419 >/dev/null 2>&1; then
          echo "Documentation server already running at http://localhost:6419"
        else
          cd "$docs_dir" && nohup "$grip_cmd" --quiet >/dev/null 2>&1 &
          sleep 2  # Wait for server to start
        fi
        nohup "$real_browser" "http://localhost:6419" >/dev/null 2>&1 &
        disown
        return 0
      else
        printyellow "Could not install grip. Falling back to terminal viewer..."
        # Set TERM to ensure proper rendering
        export TERM=${TERM:-xterm-256color}
        cd "$docs_dir" && glow -s dark INDEX.md | less -R
        return 0
      fi
    fi
    
    # No real browser found, use surf/netsurf as last resort
    printyellow "Note: Using minimal browser ($browser_cmd). For better experience, install Firefox:"
    printyellow "  sudo apt install firefox       (Ubuntu)"
    printyellow "  sudo apt install firefox-esr   (Debian)"
    printyellow "  sudo apk add firefox-esr       (Alpine)"
    echo ""
    
    if [[ "$browser_cmd" == "surf" ]]; then
      printcyan "Surf keyboard shortcuts:"
      echo "  • Ctrl+H - Go to homepage (INDEX.md)"
      echo "  • Ctrl+G - Enter URL manually"
      echo "  • Ctrl+F - Find in page"
      echo "  • Ctrl+Shift+R - Reload"
      echo ""
      printyellow "Tip: surf has limited navigation. Consider installing Firefox for a better experience."
    else
      printcyan "Using netsurf - look for navigation buttons in the toolbar"
      printyellow "Tip: For a better experience, install Firefox"
    fi
    
    # Check if grip server is already running
    if lsof -ti:6419 >/dev/null 2>&1; then
      nohup "$browser_cmd" "http://localhost:6419" >/dev/null 2>&1 &
      disown
