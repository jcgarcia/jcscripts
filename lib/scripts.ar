  echo -e "${BOLD}${MAGENTA}$1${RESET}" | sed 's/\x1b(B//g'
}

printcyan() {
  echo -e "${BOLD}${CYAN}$1${RESET}" | sed 's/\x1b(B//g'
}

printwhite() {
  echo -e "${BOLD}${WHITE}$1${RESET}" | sed 's/\x1b(B//g'
}

# Functions for printing messages in bright colors
