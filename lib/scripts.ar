  - bash (shell)
  
  Alpine Linux specific:
  - file (file type detection)
  - findutils (GNU find utilities)
  - groff (documentation formatter)
  - less (pager)

SUPPORTED DISTRIBUTIONS:
  - Ubuntu/Debian (apt)
  - Fedora/Red Hat/CentOS (dnf/yum)
  - Arch Linux (pacman)
  - Alpine Linux (apk)

EXAMPLES:
  installextras           # Install all required extras
  installextras -h        # Show this help message
  installextras -v        # Show version information

AUTHOR:
  Julio Cesar Garcia

EOF
}

# Function to extract the version from the script header of the current script
extract_version() {
    local script_file="$0"  # Referring to the current script
    grep -i '^# Version:' "$script_file" | awk -F': ' '{print $2}'
}

# Helper function to display the version
show_version() {
    version=$(extract_version)
    echo "installextras version: ${version}"
}

# Detect OS type
detect_os() {
  if [[ -f /etc/os-release ]]; then
    . /etc/os-release
    OS_ID="$ID"
    OS_VERSION="$VERSION_ID"
    OS_NAME="$NAME"
  elif [[ -f /etc/lsb-release ]]; then
    . /etc/lsb-release
    OS_ID="$DISTRIB_ID"
    OS_VERSION="$DISTRIB_RELEASE"
    OS_NAME="$DISTRIB_DESCRIPTION"
  else
    echo "ERROR: Unable to detect OS type."
    exit 1
  fi
  
  echo "Detected OS: $OS_NAME ($OS_ID)"
}

# Check if a command exists
command_exists() {
  command -v "$1" &> /dev/null
}

# Install packages for Ubuntu/Debian
install_ubuntu_debian() {
  echo ""
  echo "Installing packages for Ubuntu/Debian-based system..."
  echo ""
  
  local packages=(
    "git"
    "binutils"
    "util-linux"
    "ncurses-bin"
    "jq"
    "curl"
    "unzip"
    "xdg-utils"
  )
  
  echo "Updating package index..."
  sudo apt update
  
  echo "Installing packages: ${packages[*]}"
  sudo apt install -y "${packages[@]}"
  
  echo ""
  echo "✓ Ubuntu/Debian packages installed successfully!"
}

# Install packages for Fedora/Red Hat/CentOS
install_fedora_rhel() {
  echo ""
  echo "Installing packages for Fedora/Red Hat-based system..."
  echo ""
  
  local packages=(
    "git"
    "binutils"
    "util-linux"
    "ncurses"
    "jq"
    "curl"
    "unzip"
    "xdg-utils"
  )
  
  if command_exists dnf; then
    echo "Installing packages: ${packages[*]}"
    sudo dnf install -y "${packages[@]}"
  elif command_exists yum; then
    echo "Installing packages: ${packages[*]}"
    sudo yum install -y "${packages[@]}"
  else
    echo "ERROR: Neither dnf nor yum found."
    exit 1
  fi
  
  echo ""
  echo "✓ Fedora/Red Hat packages installed successfully!"
}

# Install packages for Arch Linux
install_arch() {
  echo ""
  echo "Installing packages for Arch-based system..."
  echo ""
  
  local packages=(
    "git"
    "binutils"
    "util-linux"
    "ncurses"
    "jq"
    "curl"
    "unzip"
    "xdg-utils"
  )
  
  echo "Updating package database..."
  sudo pacman -Sy
  
  echo "Installing packages: ${packages[*]}"
  sudo pacman -S --noconfirm --needed "${packages[@]}"
  
  echo ""
  echo "✓ Arch Linux packages installed successfully!"
}

# Install packages for Alpine Linux
install_alpine() {
  echo ""
  echo "Installing packages for Alpine Linux..."
  echo ""
  echo "NOTE: Alpine Linux requires additional packages for full compatibility."
  echo ""
  
  local packages=(
    "git"
    "binutils"
    "util-linux"
    "ncurses"
    "jq"
    "curl"
    "unzip"
    "bash"
    "file"
    "findutils"
    "groff"
    "less"
    "xdg-utils"
  )
  
  echo "Updating package index..."
  sudo apk update
  
  echo "Installing packages: ${packages[*]}"
  sudo apk add --no-cache "${packages[@]}"
  
  echo ""
  echo "✓ Alpine Linux packages installed successfully!"
  echo ""
  echo "IMPORTANT for Alpine users:"
  echo "  - Bash has been installed (scripts require bash)"
  echo "  - Make sure scripts use #!/bin/bash shebang"
  echo "  - 'file' command installed for proper file detection"
  echo "  - 'findutils' provides GNU-compatible find command"
}

# Verify installations
verify_installations() {
  echo ""
  echo "========================================"
  echo "   Verifying Installations"
  echo "========================================"
  echo ""
  
  local tools=(
    "git:Git"
    "ar:Binutils (ar)"
    "column:Util-linux (column)"
    "tput:Ncurses (tput)"
    "jq:JQ (JSON processor)"
    "curl:Curl"
    "unzip:Unzip"
    "xdg-open:XDG-Utils (xdg-open)"
  )
  
  local missing=()
  
  for tool in "${tools[@]}"; do
    local cmd="${tool%%:*}"
    local name="${tool##*:}"
    
    if command_exists "$cmd"; then
      echo "✓ $name: installed"
    else
      echo "✗ $name: NOT FOUND"
      missing+=("$name")
    fi
  done
  
  # Alpine-specific checks
  if [[ "$OS_ID" == "alpine" ]]; then
    echo ""
    echo "Alpine-specific tools:"
    
    for tool in "file:File" "find:Findutils" "bash:Bash"; do
      local cmd="${tool%%:*}"
      local name="${tool##*:}"
      
      if command_exists "$cmd"; then
        echo "✓ $name: installed"
      else
        echo "✗ $name: NOT FOUND"
        missing+=("$name")
      fi
    done
  fi
  
  echo ""
  
  if [[ ${#missing[@]} -eq 0 ]]; then
    echo "✓ All required tools are installed!"
    return 0
  else
    echo "⚠ WARNING: Some tools are missing: ${missing[*]}"
    echo "Please check the installation logs above for errors."
    return 1
  fi
}

# Main function
main() {
  # Handle command-line options
  case "${1:-}" in
    -h|--help)
      show_help
      exit 0
      ;;
    -v|--version)
      show_version
      exit 0
      ;;
    -*)
      echo "ERROR: Unknown option: $1"
      echo "Use -h or --help for usage information."
      exit 1
      ;;
  esac
  
  echo "========================================"
  echo "   JC Scripts - Install Extras"
  echo "========================================"
  echo ""
  
  # Detect OS
  detect_os
  
  # Confirm installation
  echo ""
  read -p "Install all required extras for jcscripts? (y/n): " confirm
  if [[ "$confirm" != "y" && "$confirm" != "Y" ]]; then
    echo "Installation cancelled."
    exit 0
  fi
  
  # Install based on detected OS
  case "$OS_ID" in
    ubuntu|debian)
      install_ubuntu_debian
      ;;
    fedora|rhel|centos|rocky|almalinux)
      install_fedora_rhel
      ;;
    arch|manjaro)
      install_arch
      ;;
    alpine)
      install_alpine
      ;;
    *)
      echo ""
      echo "ERROR: Unsupported distribution: $OS_ID"
      echo "Supported distributions: Ubuntu, Debian, Fedora, RHEL, CentOS, Arch, Alpine"
      echo ""
      echo "Please install the following packages manually:"
      echo "  - git, binutils, util-linux, ncurses, jq, curl, unzip"
      exit 1
      ;;
  esac
  
  # Verify installations
  verify_installations
  
  echo ""
  echo "========================================"
  echo "   Installation Complete!"
  echo "========================================"
  echo ""
  echo "All required extras have been installed."
  echo "You can now proceed with installing the jcscripts collection."
  echo ""
  echo "Next steps:"
  echo "  1. Run: ./installscripts"
  echo "  2. Install AWS CLI (optional): ./awsinstall"
  echo ""
}

# Run main function
main "$@"
