#!/bin/bash

# ------------------------------------------------------------------------------
# Script Name:    wslfunctions
# Description:    Library of functions to manage WSL distributions.
# Author:         Julio Cesar Garcia
# Date:           20/03/2025
# Version:        1.0
# ------------------------------------------------------------------------------

# Functions in this library:
# - convert_to_windows_path: Convert a Linux path to a Windows path.
# - get_wsl_path: Get the path to the `wsl.exe` executable.
# - get_powershell_path: Get the path to the `powershell.exe` executable.
# - list_distributions: List all WSL distributions.
# - list_online_numbered_distributions: List available WSL distributions online.
# - install_distro: Install a WSL distribution.
# - list_mumbered_installed_distributions: List installed WSL distributions with numbers.
# - is_distro_running: Check if a WSL distribution is running.
# - terminate_distro: Terminate a running WSL distribution.
# - unregister_distro: Unregister a WSL distribution.
# - validate_distro: Validate the existence of a WSL distribution.
# - get_default_distro: Get the default WSL distribution.
# - get_current_distro: Get the currently running WSL distribution.
# - get_active_distro: Get the active WSL distribution.
# - select_distro: Prompt the user to select a WSL distribution.
# - get_windows_user: Detect the Windows username.
# - clean_residual_files: Remove residual files for a WSL distribution.
# - restore_distro: Restore a WSL distribution from a backup.
# - backup_distro_function: Backup a WSL distribution.
# - install_distro_function: Install a WSL distribution interactively.
# - remove_distro_function: Remove a WSL distribution interactively.

# Source the colorfunctions library
source "$HOME/.scripts/colorfunctions"

convert_to_windows_path() {
  local linux_path="$1"
  # Remove the "/mnt/" prefix and replace "/" with "\\"
  local windows_path
  windows_path=$(echo "$linux_path" | sed 's/^\/mnt\///' | sed 's/\//\\\\/g')

  # Ensure the drive letter is uppercase
  windows_path=$(echo "$windows_path" | sed 's/^\([a-z]\)/\U\1/')
  
  echo "$windows_path"
}

# Get the path to the wsl.exe executable
get_wsl_path() {
  local wsl_path
  wsl_path=$(type -p wsl.exe)

  if [[ -z "$wsl_path" ]]; then
    echo "ERROR: Unable to locate wsl.exe. Please ensure it is installed and accessible."
    exit 1
  fi

  echo "$wsl_path"
}

# Get the path to powershell.exe
get_powershell_path() {
  local powershell_path
  powershell_path=$(type -p powershell.exe)

  if [[ -z "$powershell_path" ]]; then
    echo "ERROR: Unable to locate powershell.exe. Please ensure it is installed and accessible."
    exit 1
  fi

  echo "$powershell_path"
}

# List WSL distributions
list_distributions() {
  # echo "Processing and cleaning the distribution list..."

  /mnt/c/Windows/System32/wsl.exe --list \
    | sed '1d' \
    | sed 's/ (Default)//g' \
    | tr -cd '\11\12\15\40-\176' \
    | tr '*' ' ' \
    | sed '/^$/d' \
    | awk '{$1=$1; print}'
}

# Display available distributions online with numbered options
list_online_numbered_distributions() {
  printcyan "Fetching list of available distributions online..."
  local distro_list

  # Fetch, clean, and filter the distribution list
  distro_list=$(/mnt/c/Windows/System32/wsl.exe --list --online 2>/dev/null | tr -d '\0' | sed '/^NAME/,$!d' | sed '/^NAME/d' | sed '/^$/d')

  if [[ -z "$distro_list" ]]; then
    printred "ERROR: Unable to fetch the list of available distributions online."
    exit 1
  fi

  # Format and number the valid distribution list for user selection
  printcyan "Available Distributions:"
  echo "$distro_list" | nl -w2 -s". "
}

# Install a WSL distribution
install_distro() {
  local distro="$1"
  local custom_name="$2"

  if [[ -z "$custom_name" ]]; then
    echo "Installing '$distro'..."
    powershell.exe -Command "wsl.exe --install -d \"$distro\""
  else
    echo "Installing '$distro' with the custom name '$custom_name'..."
    powershell.exe -Command "wsl.exe --install -d \"$distro\" --name \"$custom_name\""
  fi

  if [[ $? -ne 0 ]]; then
    echo -e "${BOLD}${RED}ERROR: Failed to install '$distro'.${RESET}"
    exit 1
  fi

  echo "Successfully installed '$distro'."
}



# List all installed distributions with numbered options
list_mumbered_installed_distributions() {
  echo "Fetching the list of installed distributions..."
  local distro_list

  # Fetch and clean the distribution list
  distro_list=$(/mnt/c/Windows/System32/wsl.exe --list --verbose 2>/dev/null | tr -d '\0' | tr '*' ' ' | sed '1d')

  if [[ -z "$distro_list" ]]; then
    echo -e "${BOLD}${RED}ERROR: Unable to fetch the list of installed distributions.${RESET}"
    exit 1
  fi

  # Format and number the valid distribution list for user selection
  echo "Installed Distributions:"
  # Print each line with its number, properly formatted
  local i=1
  while IFS= read -r line; do
    # Remove extra spaces and tabs, collapse multiple spaces
    line=$(echo "$line" | awk '{$1=$1; print}')
    if [[ -n "$line" ]]; then
      printf "%2d. %s\n" "$i" "$line"
      ((i++))
    fi
  done <<< "$distro_list"
}

# Check if a distribution is currently running
is_distro_running() {
  local distro="$1"
  /mnt/c/Windows/System32/wsl.exe --list --verbose | grep "$distro" | grep -q "Running"
}

# Terminate a running distribution
terminate_distro() {
  local distro="$1"
  echo "Terminating distribution '$distro'..."
  /mnt/c/Windows/System32/wsl.exe --terminate "$distro"
}

# Unregister a WSL distribution
unregister_distro() {
  local distro="$1"
  printyellow "Unregistering distribution '$distro'..."
  /mnt/c/Windows/System32/wsl.exe --unregister "$distro" || {
    printred "ERROR: Failed to unregister distribution '$distro'."
    exit 1
  }
  printgreen "Successfully unregistered distribution '$distro'."
}


# Validate distribution existence
validate_distro() {
  local distro="$1"

  # Fetch sanitized list
  local distro_list
  distro_list=$(list_distributions)

  # Ensure the list is not empty
  if [[ -z "$distro_list" ]]; then
    echo -e "${BOLD}${RED}ERROR: Failed to retrieve a valid distribution list.${RESET}"
    exit 1
  fi

  # Strip (Default) from the list
  distro_list=${distro_list// \(Default\)/}

  # Compare sanitized distro input with each line
  while IFS= read -r line; do
    line=$(echo "$line" | tr -d '\r\n') # Sanitize line
    if [[ "$line" == "$distro" ]]; then
      return 0
    fi
  done <<< "$distro_list"

  echo -e "${BOLD}${RED}ERROR: Distribution '$distro' not found.${RESET}"
  exit 1
}

# Get the name of the currently running distribution
get_default_distro() {
  /mnt/c/Windows/System32/wsl.exe --list --verbose \
    | grep '*' \
    | awk '{print $1}' \
    | sed 's/ (Default)//g'
}

# Get the name of the currently running distribution
get_current_distro() {
  if [[ -n "$WSL_DISTRO_NAME" ]]; then
    echo "$WSL_DISTRO_NAME"
  else
    echo "No distro detected"
    exit 1
  fi
}



# Get the currently running distribution using $WSL_DISTRO_NAME
get_active_distro() {
  if [[ -n "$WSL_DISTRO_NAME" ]]; then
    echo "$WSL_DISTRO_NAME"
  else
    echo -e "${BOLD}${RED}ERROR: Unable to detect active distribution. WSL_DISTRO_NAME is not set.${RESET}"
    exit 1
  fi
}

# Check if a distribution is currently running
is_distro_running() {
  local distro="$1"
  /mnt/c/Windows/System32/wsl.exe --list --verbose | grep "$distro" | grep -q "Running"
}

# Prompt the user to select a WSL distribution
select_distro() {
  list_installed_distributions
  echo -n "Enter the number of the distribution to back up: "
  read -r selection

  if [[ -z "$selection" || ! "$selection" =~ ^[0-9]+$ ]]; then
    echo -e "${BOLD}${RED}ERROR: Invalid input. Operation aborted.${RESET}"
    exit 1
  fi

  # Extract the selected distribution name
  local distro_list
  distro_list=$(/mnt/c/Windows/System32/wsl.exe --list --verbose 2>/dev/null | sed '1d')
  local distro
  distro=$(echo "$distro_list" | sed -n "${selection}p" | awk '{$1=$1; print $1}')  # Extract only the first word (distro name)

  if [[ -z "$distro" ]]; then
    echo -e "${BOLD}${RED}ERROR: Invalid selection. Operation aborted.${RESET}"
    exit 1
  fi
  echo "$distro"
}

# Detect the Windows username
get_windows_user() {
  local powershell_path
  powershell_path=$(get_powershell_path)

  local win_user
  win_user=$("$powershell_path" -Command "whoami" | tr -d '\r\n' | awk -F'\\' '{print $2}')
  if [[ -z "$win_user" ]]; then
    echo "ERROR: Unable to determine the Windows username."
    exit 1
  fi
  echo "$win_user"
}

# Unregister a WSL distribution 
unregister_distro() {
  local distro="$1"

  # Validate that a distro name is provided
  if [[ -z "$distro" ]]; then
    echo "ERROR: No distribution specified to unregister."
    return 1
  fi

  # Dynamically find the path to wsl.exe
  local wsl_path
  wsl_path=$(type -p wsl.exe)

  if [[ -z "$wsl_path" ]]; then
    echo "ERROR: Unable to locate wsl.exe. Please ensure it is installed and accessible."
    exit 1
  fi

  echo "Checking if '$distro' is registered in WSL..."
  if "$wsl_path" --list --verbose | grep -q "^$distro "; then
    echo "Unregistering distribution '$distro'..."
    "$wsl_path" --unregister "$distro"
    if [[ $? -ne 0 ]]; then
      echo "ERROR: Failed to unregister distribution '$distro' using wsl.exe."
      return 1
    fi
    echo "Successfully unregistered distribution '$distro' using wsl.exe."
  else
    echo "INFO: Distribution '$distro' is not currently registered in WSL."
  fi

  # Double-check and use wslconfig.exe as a fallback
  echo "Ensuring '$distro' is fully unregistered using wslconfig.exe..."
  local wslconfig_path
  wslconfig_path=$(type -p wslconfig.exe)

  if [[ -z "$wslconfig_path" ]]; then
    echo "ERROR: Unable to locate wslconfig.exe. Please ensure it is installed and accessible."
    return 1
  fi

  "$wslconfig_path" /u "$distro"
  if [[ $? -ne 0 ]]; then
    echo "ERROR: Failed to unregister distribution '$distro' using wslconfig.exe."
    return 1
  fi

  echo "Successfully removed all traces of '$distro'."
  return 0
}

# Remove residual files for a distribution
clean_residual_files() {
  local distro="$1"
  local win_user
  win_user=$(get_windows_user)

  # Dynamically determine the path to powershell.exe
  local powershell_path
  powershell_path=$(type -p powershell.exe)

  if [[ -z "$powershell_path" ]]; then
    echo "ERROR: Unable to locate powershell.exe. Please ensure it is installed and accessible."
    exit 1
  fi

  echo "Checking for residual files of '$distro' for Windows user '$win_user'..."
  "$powershell_path" -NoProfile -Command "
    \$distroName = '${distro}'
    \$userPackagesPath = 'C:\\Users\\${win_user}\\AppData\\Local\\Packages'

    # Find matching directories
    \$matches = Get-ChildItem -Path \$userPackagesPath -Recurse -ErrorAction SilentlyContinue |
                Where-Object { \$_ -and \$_.FullName -like \"*\$distroName*\" }

    if (\$matches) {
        # Remove matched directories
        \$matches | ForEach-Object {
            try {
                Remove-Item -Path \$_.FullName -Recurse -Force -ErrorAction SilentlyContinue
                Write-Host \"Removed: \$_\"
            } catch {
                Write-Error \"Failed to remove: \$_.FullName - \$_.Exception.Message\"
            }
        }
    } else {
        Write-Host \"No residual files found for '\$distroName'.\"
    }
  " || {
    echo -e "${BOLD}${RED}ERROR: Failed to clean residual files for '$distro'.${RESET}"
    exit 1
  }

  echo -e "${BOLD}${CYAN}Successfully cleaned residual files for '$distro'.${RESET}"
}

# Restore the WSL distribution
restore_distro() {
  local distro="$1"
  local backup_file="$2"
  local backup_file_windows=$(wslpath -w "$backup_file")
  local install_location_windows
  install_location_windows="C:\\Users\\$(get_windows_user)\\AppData\\Local\\$distro"
  local powershell_path
  powershell_path=$(type -p powershell.exe)
  wsl_path=$(type -p wsl.exe)
  win_wsl_path=$(wslpath -w "$wsl_path")

  # use it as  "$powershell_path" -Command or "$wsl_path" --list --verbose 

  # Ensure the log directory exists
  mkdir -p "$LOG_DIR"

echo "Debug: About to restore '$distro' from '$backup_file_windows to $install_location_windows'..."

  echo "Restoring '$distro' from '$backup_file_windows'..."
  local log_file="$LOG_DIR/${distro}_restore.log"
  "$powershell_path" -Command \
    ""$win_wsl_path" --import \"$distro\" \"$install_location_windows\" \"$backup_file_windows\"" 2>&1 | tee "$log_file"

  if [[ ${PIPESTATUS[0]} -ne 0 ]]; then
    echo -e "${BOLD}${RED}ERROR: Failed to restore '$distro'. Check the log file at '$log_file' for more details.${RESET}"
    exit 1
  fi

  echo "Restore completed successfully for '$distro'. Log file stored at '$log_file'."
}

