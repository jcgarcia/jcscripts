    return 1
  fi
  
  echo "Updating package database..."
  sudo pacman -Sy
  
  echo "Installing surf..."
  sudo pacman -S --noconfirm surf
  
  if command -v surf &> /dev/null; then
    echo "✓ surf installed successfully!"
    echo "surf is a lightweight WebKit-based browser perfect for X11 forwarding."
    return 0
  else
    echo "ERROR: surf installation failed."
