    # Ask about test apps
    read -p "Install X11 test applications (xeyes, xclock, etc.)? (Y/n) " -r
    echo
    if [[ $REPLY =~ ^[Nn]$ ]]; then
        INSTALL_TEST_APPS=false
    fi
    
    # Ask about SSH restart
    read -p "Automatically restart SSH service after configuration? (Y/n) " -r
    echo
    if [[ $REPLY =~ ^[Nn]$ ]]; then
        RESTART_SSH=false
    fi
    
    echo ""
    read -p "Proceed with installation? (Y/n) " -r
    echo
    if [[ $REPLY =~ ^[Nn]$ ]]; then
        print_warning "Installation cancelled"
        exit 0
    fi
}

# ------------------------------------------------------------------------------
# Main Execution
# ------------------------------------------------------------------------------

main() {
  # Handle command-line options first
  case "${1:-}" in
    -h|--help)
      show_help
      exit 0
      ;;
    -v|--version)
      show_version
      exit 0
      ;;
    -*)
      echo "ERROR: Unknown option: $1"
      echo "Use -h or --help for usage information."
      exit 1
      ;;
  esac

  # Clear screen for better readability (only if interactive)
  if [ -t 0 ]; then
    clear
  fi
    
    print_header "X11 Forwarding Setup for Ubuntu/Alpine Server"
    
    # Pre-flight checks
    check_root
    check_os
    
    # Interactive prompts (comment out for non-interactive mode)
    if [ -t 0 ]; then
        interactive_setup
    fi
    
    # Execute setup steps
    update_system
    install_x11_packages
    backup_ssh_config
    configure_ssh_x11
    verify_ssh_config
    restart_ssh_service
    setup_user_environment
    configure_firewall
    
    # Run verification tests
    run_tests
    
    # Display completion message
    print_instructions
    show_rollback_instructions
    
    
    print_success "Setup completed successfully!"
}

# Run main function
main "$@"

