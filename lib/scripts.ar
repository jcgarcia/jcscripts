        fi
      fi
    else
      printcyan "ℹ Public repository does not exist (may have been deleted already)"
    fi
  fi
  
  # Delete local project directory
  printcyan "Deleting local project directory..."
  cd ..
  
  if rm -rf "$project_dir"; then
    printgreen "✓ Local directory deleted: $project_dir"
  else
